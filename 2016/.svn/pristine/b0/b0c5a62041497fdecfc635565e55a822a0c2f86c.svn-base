body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td{margin:0;padding:0}
article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section{display:block}
fieldset,img{border:0}
table{border-collapse:collapse;border-spacing:0}
li{list-style:none}
a,a:hover{text-decoration:none;color:#ffe400;}
a, textarea, input, button { outline: none }

/** 清楚浮动 **/
.clearfix:after { visibility: hidden; display: block; font-size: 0; content: " "; clear: both; height: 0; }
* html .clearfix { zoom: 1; }

* { -webkit-tap-highlight-color: transparent; -webkit-appearance: none; }
html{
    -ms-touch-action: none;
    font-size: 62.5%;
}
html,body{ height: 100%;}
body{line-height:1.5;font-size:12px;font-family:"Microsoft Yahei",Arial,Helvetica,sans-serif,Arial;color:#424240;-webkit-tap-highlight-color:rgba(0,0,0,0);} 


input,select,button{
    -webkit-appearance:none;
    appearance:none; 
    border-radius: 0; 
}

input[type="submit"],
input[type="reset"],
input[type="button"],
input{
    -webkit-appearance:none;
}

.wrap{position:relative;color:#fff; height: 100%; overflow: hidden;}

body{
    background-image: url(../images/ll-img-6.jpg);
    background-color: #aec6ec;
    background-size: 100% auto;
    background-position: center top; 
    background-repeat: no-repeat;
}

.hide {display: none;}

.fade{
    background: rgba(0,0,0,.6);
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 3;
}

.audio-item{
    opacity: 0;
}


/*页面加载提示*/
.mod_loading{
    position:absolute;
    height:100%;
    left:0;
    top:0;
    width:100%;
    z-index:100;
    background-color: #cadcf3;
    display:-webkit-box;
    -webkit-box-align:center;
}
.mod_loading .content{ text-align:center;-webkit-box-flex:1;margin:0 80px;}
.mod_loading .content .progress{height:13px;-webkit-border-radius:17px;border-radius:17px;background-color:#fff; position: relative;}
.mod_loading .content .progress span{display:block;height:13px;-webkit-border-radius:17px;border-radius:17px;background-color:#9fc4f3;}

.mod_loading .content .progress span{-webkit-transition:width 0.4s linear;}
.mod_loading .content  #progress_num{ display: block; text-align: center; line-height:34px;color:#fff; height: 34px; width: 100%; }
.mod_loading .content .progress .load-icon{
    display: block;
    width: 37px;
    height: 45px;
    background-image: url(../images/load-icon.png);
    background-repeat: no-repeat;
    background-size: contain;
    position: absolute;
    top: 50%;
    left: 0;
    margin-top: -30px;
    margin-left: -18px;
}


.lancom-page .item, 
.lancom-page .item .box {width: 100%;height: 100%;overflow: hidden;}

.lancom-page .item img{
    width: 100%;
    display: block;
}

/*common*/
.lancom-page .img-change{
    position: relative;
}
.lancom-page .img-change .cur{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    display: none;
}


.lancom-page .btn-item{
    width: 100%;
    position: fixed;
    bottom: 5%;
    left: 0;
    z-index: 4;
}
.lancom-page .btn-item .area-btn{
    display: block;
    margin: 0 auto;
    width: 55.73%;
    height: 0;
    padding-top: 13.47%;
    background-repeat: no-repeat;
    background-size: contain;
}
.lancom-page .largen-item .area-btn{
    background-image: url(../images/btn-item/largen/btn-bg.png);
}
.lancom-page .largen-item .area-btn:active{
    background-image: url(../images/btn-item/largen/btn-bg-active.png);
}
.lancom-page .restore-item .area-btn{
    background-image: url(../images/btn-item/restore/btn-bg.png);
}
.lancom-page .restore-item .area-btn:active{
    background-image: url(../images/btn-item/restore/btn-bg-active.png);
}

.arrow{
    display: none;
    position: fixed;
    bottom: 3%;
    left: 50%;
    margin-left: -5%;
    background: url(../images/mod-top-btn.png);
    background-size: contain;
    width: 10%;
    height: 0;
    padding-top: 8.54%;
    -webkit-animation: slide_up 2s infinite ease-out;
}

.mod-animation{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-repeat: no-repeat;
    background-size: contain;
    display: none;
}
.largen-animation{
    -webkit-animation: largen-step 0.8s linear;
}
.restore-animation .path-top{
    position: absolute;
    top: 0;
    left: 0;
    display: block;
    width: 100%;
    height: 100%;
    -webkit-animation: restore-step-Top 0.6s linear;
    background-repeat: no-repeat;
    background-size: contain;
    background-position: center top;
}
.restore-animation .path-btm{
    position: absolute;
    top: 0;
    left: 0;
    display: block;
    width: 100%;
    height: 100%;
    -webkit-animation: restore-step-Btm 0.6s linear;
    background-repeat: no-repeat;
    background-size: contain;
    background-position: center bottom;
}

@-webkit-keyframes largen-step{
    0%, 11.6%{  
        background-image: url(../images/animation/largen/1.png);
    }
    12.6%, 24.2%{ 
        background-image: url(../images/animation/largen/2.png);
    }
    25.2%, 36.8%{
        background-image: url(../images/animation/largen/3.png);
    }
    37.8%, 49.4%{
        background-image: url(../images/animation/largen/4.png);
    }
    50.4%, 62%{
        background-image: url(../images/animation/largen/5.png);
    }
    63%, 74.6%{
        background-image: url(../images/animation/largen/6.png);
    }
    75.6%, 87.2%{
        background-image: url(../images/animation/largen/7.png);
    }
    88.2%, 100%{
        background-image: url(../images/animation/largen/8.png);
    }
}

@-webkit-keyframes restore-step-Top{
    0%, 32.7%{
        background-image: url(../images/animation/restore/1-top.png);
    }
    33.7%, 66.4%{
        background-image: url(../images/animation/restore/2-top.png);
    }
    67.4%, 100%{
        background-image: url(../images/animation/restore/3-top.png);
    }
}

@-webkit-keyframes restore-step-Btm{
    0%, 32.7%{
        background-image: url(../images/animation/restore/1-btm.png);
    }
    33.7%, 66.4%{
        background-image: url(../images/animation/restore/2-btm.png);
    }
    67.4%, 100%{

    }
}




