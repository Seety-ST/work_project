}


/* tips pop */
.pop-tipsItem{
	width: 70%;
	position: fixed;
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%,-50%); 
	z-index: 5;
	text-align: center;
}
.pop-tipsItem .text-item{
	border-top-left-radius: 10px;
	border-top-right-radius: 10px;
	background: #f8f9f9;
	background-image: -webkit-gradient(linear,0% 0%, 0% 100%, from(#faf9f9), to(#f8f9f9));
	padding: 8% 7%;
	margin-bottom: 1px;
	font-size: 1.3rem;
}
.pop-tipsItem .btn-item{
	border-bottom-left-radius: 10px;
	border-bottom-right-radius: 10px;
	background: #f9f9f9;
	background-image: -webkit-gradient(linear,0% 0%, 0% 100%, from(#F9F9F9), to(#F9F9F9), color-stop(0.5,#F9FAF9));
	font-size: 1.5rem;
}
.pop-tipsItem .btn-item .area-btn{
	display: block;
	padding: 4% 5%;
	color: #0982ff;
}
.pop-tipsItem .btn-item .area-btn:active{
	opacity: 0.65;
}

/* content */
.login-page{
	padding: 12.933% 6.67%  9.867%;
}
.login-page h1.logo-item{
