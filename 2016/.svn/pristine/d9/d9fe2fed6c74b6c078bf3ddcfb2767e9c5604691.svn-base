    color: #fff;
    font-size: 0.2048rem;
}
html, body {
    font-family: 'Microsoft YaHei', 'Verdana', 'SimSun';
    height: 100%;
    width: 100%;
}
input,select,button{
    -webkit-appearance:none;
    appearance:none;
    border-radius: 0;
}
input[type="submit"],
input[type="reset"],
input[type="button"],
input[type="radio"],
input[type="radio"]:checked,
button{
    -webkit-appearance:none;
    appearance:none;
    border-radius: 0;
}
.dn{
    display: none;
}

/* common */
/* lazyload css */
.lazyload,
.lazyloading {
    opacity: 0;
}
.lazyloaded {
    opacity: 1;
    transition: opacity 300ms;
}

.lazyload {
  background: #212528;
}

.lazyloading {
  background: #212528;
  min-height: 60px;
}

.teaser.lazyload {
    opacity: 0;
    transform: scale(0.95);
}

.teaser.lazyloaded {
    opacity: 1;
    transform: scale(1);
    transition: all 600ms;
}

