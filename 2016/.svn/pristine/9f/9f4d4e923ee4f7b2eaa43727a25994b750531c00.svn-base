html, body {
	position: relative;
	height: 100%;
}

#circle-page{
	min-width: 1024px;
}

body { 
	font: 0.14rem "Microsoft Yahei",Arial,Helvetica,sans-serif; 
	color: #fff;
	background: #bdb6db;
	-webkit-backface-visibility: hidden;
	-webkit-transform-style: preserve-3d;
}

.header{
	z-index: 10;
}
.header,
.mod-nav-header .nav a{
	color: #fff;
}
.header .nav  li a:hover .n-line,
.header .nav  li.active a .n-line,
.header .nav  li.active a:hover .n-line,
.mod-nav-header .nav .block{
	background-color: #fff; 
}

.footer{
	color: #fff;
}
.footer a{
	color: #fff;
}


.section { 
	background-repeat: no-repeat;
	width: 100%;
	height: 100%;
}
.section img{
	width: 100%;
	display: block;
}
.section .con-item{
	width: 100%;
	height: 100%;
	overflow: hidden;
	position: relative;
}
.section .bg-item{
	width: 100%;
	height: 100%;
	-webkit-animation-name: fadeOut;
	animation-name: fadeOut;
}
.section .antinode-item{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 12rem;
	-webkit-animation-name: fadeOut;
	animation-name: fadeOut;
}
.section .info-item{
	position: absolute;
	top: 10%;
	left: 0%;
	width: 100%;
	height: 100%;
	-webkit-animation-name: fadeOut;
	animation-name: fadeOut;
}

.contain { 
	position: relative; 
	width: 100%;
	height: 100%;

}

.animated {
	-webkit-animation-duration: 0.3s;
	animation-duration: 0.3s;
	-webkit-animation-fill-mode: both;
	animation-fill-mode: both;
}
.active .animated {
	-webkit-animation-duration: 1s;
	animation-duration: 1s;
}

.mod-logo{
	width: 2.18rem;
	position: fixed;
	top: 0.26rem;
	left: 1.06rem;
	z-index: 9;
}
.mod-logo img{
	display: block;
	width: 100%;
}

.mod-download-item{
	
}
.mod-download-item .code-item{
	float: left;
	width: 37%;
	overflow: hidden;
	background: #fff;
	padding: 1.25%;
}
.mod-download-item .code-item img{
	width: 100%;
	display: block;
}
.mod-download-item .btn-warp{
	float: right;
	width: 50%;
}
.mod-download-item .btn-warp .btn{	
	display: block;
	border: 2px solid #96a8d0;
	width: 100%;
	padding: 6% 0;
	line-height: 1.2;
	border-radius: 31px;
	margin-bottom: 0.1rem;
	font-size: 0;
	color: #96a8d0;
	-webkit-transition: 600ms;
	-moz-transition: 600ms;
	-ms-transition: 600ms;
	-o-transition: 600ms;
	transition: 600ms;
}
.mod-download-item .btn-warp .btn:hover{
	color: #ffeeef;
	background: #96a8d0;
	text-decoration: none;
	-webkit-transition: 600ms;
	-moz-transition: 600ms;
	-ms-transition: 600ms;
	-o-transition: 600ms;
	transition: 600ms;
}
.mod-download-item .btn-warp .btn .icon{
	float: left;
	width: 17.65%;
	height: 0;
	padding-top: 20.6%;
	background-repeat: no-repeat;
	background-size: 100% 100%;
	margin: 0 5.8% 0 13%;
}
.mod-download-item .btn-warp .btn-iphone .icon{
	background-image: url(../images/download-icon/ios-icon-30x35.png);
}
.mod-download-item .btn-warp .btn-iphone:hover .icon{
	background-image: url(../images/download-icon/ios-icon-hover-30x35.png);
}
.mod-download-item .btn-warp .btn-android .icon{
	background-image: url(../images/download-icon/android-icon-30x35.png);
}
.mod-download-item .btn-warp .btn-android:hover .icon{
	background-image: url(../images/download-icon/android-icon-hover-30x35.png);
}
.mod-download-item .btn-warp .btn span{
	float: left;
	width: 48%;
	padding-top: 3%;
	font-size: 0.23rem;
	cursor: pointer;
}

#fp-nav.right{
	right: 1%;
}
#fp-nav ul li, .fp-slidesNav ul li{
	margin: 0;
