/* 注册弹出框 */
.pop_register{ background:url(../images/lj-img-23.png) no-repeat; width:651px; height:407px; z-index:999999; position:fixed; _position: absolute; top:50%; left:50%; margin-top:-203px; margin-left:-325px;
_top:expression(eval(document.compatMode &&
     document.compatMode=='CSS1Compat') ?
     documentElement.scrollTop + (document.documentElement.clientHeight-this.offsetHeight)/2 :/*IE6*/
     document.body.scrollTop + (document.body.clientHeight - this.clientHeight)/2);/*IE5 IE5.5*/
     _margin-top:0
}
.pop_register .close{ position:absolute;z-index:9999999;top:10px;right:10px;}
.pop_register .register{padding:75px 0 0 30px;font-family:Tahoma, Geneva, sans-serif;color:#898989;}
.pop_register .register td{padding:10px 0;}
.pop_register .register .inputClass{background:#f5f5f5;border:1px solid #cfcfcf;padding:0 5px;height:20px;padding-top:2px;width:244px;}
.pop_register .register .tips{ padding-left:5px;line-height:18px; color: #b3b2b2}
.pop_register .register img{ vertical-align:text-bottom;}
.pop_register .register .btn{padding-top:10px;}
.pop_register .register .txt{color:#df0011;}

/* 预告页 */
.Countdown-warp #imgbox{position:absolute;left:0;top:0;}
.Countdown-warp .max-pic{width:100%;height:100%;left:0px;top:0px;position:absolute;overflow:hidden;}

.Countdown-warp{ width:100%; height:100% }
.Countdown-warp .p2th{ width:1000px; height:446px; z-index:999999; position:fixed; _position: absolute; top:50%; left:50%; margin-top:-223px; margin-left:-500px;
_top:expression(eval(document.compatMode &&
     document.compatMode=='CSS1Compat') ?
     documentElement.scrollTop + (document.documentElement.clientHeight-this.offsetHeight)/2 :/*IE6*/
     document.body.scrollTop + (document.body.clientHeight - this.clientHeight)/2);/*IE5 IE5.5*/
     _margin-top:0 }
.Countdown-warp .p2th .time-item{ background:url(../images/ll-img-20.png) no-repeat; width:665px; height:216px;  padding:230px 0 0 294px; color:#000;_background:none; _filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(enabled='true', sizingMethod='scale', src="images/ll-img-20.png");}
.Countdown-warp .p2th .time-item li{ float:left; display:inline; width:87px; height:120px; line-height:120px; text-align:center; margin-right:64px; font-size:63px; color:#fff}
.Countdown-warp .header{ background:#fff}
.Countdown-warp .footer{ background:#050505 url(../images/ll-img-21.png?v=1) center top no-repeat; height:92px; position:absolute; left:0; bottom:0; z-index:999; width:100%}














