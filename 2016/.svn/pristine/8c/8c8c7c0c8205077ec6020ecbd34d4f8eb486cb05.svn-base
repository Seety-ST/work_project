/* btn */
.mod-download-item{
	width: 4.08rem;
	margin: 0 auto;
}
.mod-download-item .btn-area{
	display: block;
	width: 100%;
	height: 0.78rem;
	line-height: 0.78rem;
	border-radius: 0.4rem;
	text-align: center;
	color: #fff;
	font-weight: bold;
	font-size: 0.36rem;
}
.mod-download-item .btn-area:active{
	opacity: 0.8;
}
.mod-download-item .see-item{
	margin-top: 0.29rem;
}
.mod-download-item .btn-area i.icon{
	display: inline-block;
	margin-right: 0.05rem;
}
.mod-download-item .download-item .btn-area i.icon{
	width: 0.27rem;
	height: 0.34rem;
	-webkit-mask-image: url(../images/download-icon-27x34.png); 
	-webkit-mask-size: 100% 100%;
	background: #fff;
}
.mod-download-item .see-item .btn-area i.icon{
	width: 0.3rem;
	height: 0.31rem;
	-webkit-mask-image: url(../images/look-icon-30x31.png); 
	-webkit-mask-size: 100% 100%;
	background: #fff;
}
.mod-item{
	width: 100%;
	position: relative;
	overflow: hidden;
}
.mod-item .listItem{
	position: relative;
}
.mod-item .listItem .topic-item{
	position: absolute;
	-webkit-animation-name: topicAnimate; 
	-webkit-animation-duration: 1s; 
	-webkit-animation-iteration-count: infinite; 
}
.mod-item .listItem .topic-item i.icon{
	display:block;
	background-size: 100% auto;
	background-repeat: no-repeat;
	background-position: center top;
}

