/* animate */
@-webkit-keyframes topicAnimate { 
	0% { -webkit-transform: translate3d(0, 0, 10px) rotateX(15deg); opacity: 1; }
	20% { -webkit-transform: translate3d(0, -3px, 10px) rotateX(0); opacity: 1; }
	60% { -webkit-transform: translate3d(0, -6px, 10px) rotateX(0); opacity: 1; }
	80% { -webkit-transform: translate3d(0, -9px, 10px) rotateX(0); opacity: 1; }
	100% { -webkit-transform: translate3d(0, -12px, 10px) rotateX(-15deg); opacity: 1; }
}



