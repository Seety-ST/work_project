/* lazyload css */
.lazyload,
.lazyloading {
	opacity: 0;
}
.lazyloaded {
	opacity: 1;
	transition: opacity 300ms;
}

.lazyload {
  background: #fff;
}

.lazyloading {
  background: #fff;
  min-height: 20rem;
}

.teaser.lazyload {
	opacity: 0;
	transform: scale(0.95);
}

.teaser.lazyloaded {
	opacity: 1;
	transform: scale(1);
	transition: all 600ms;
}

