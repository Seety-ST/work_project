.w1000{
	margin: 0 auto;
	width: 1000px;
}

body { 
	font: 14px "Microsoft Yahei",Arial,Helvetica,sans-serif; 
	color: #fff;
	background-color: #c6a9d2;
}

.header{
	color: #fff;
}
.header .nav  li a:hover .n-line,
.header .nav  li.active a .n-line,
.header .nav  li.active a:hover .n-line,
.mod-nav-header .nav .block{
	background-color: #fff; 
}

.footer{
	color: #fff;
}
.footer a{
	color: #fff;
}

.page { 
	background-repeat: no-repeat;
}
.contain { 
	position: relative; 
	width: 100%;
	height: 100%;

}
.page .con-item{
	width: 53%;
	margin: 0 auto;
}

#navBar { 
	z-index: 3; 
	position: absolute; 
	top: 40%; 
	right: 24px;
}
#navBar .active { 
	background: url(../images/hcq/page-icon-active-12x12.png) no-repeat;
}
#navBar li { 
	cursor: pointer; 
	margin-bottom: 16px; 
	transition: all .7s ease; 
	line-height: 0px; 
	font-size: 0;
	text-align: center; 
	width: 12px; 
	height: 12px;
	background: url(../images/hcq/page-icon-12x12.png) no-repeat;
}


.mod-logo{
	width: 9.427%;
	position: fixed;
	top: 2.63%;
	left: 5.57%;
	z-index: 9;
}
.mod-logo img{
	display: block;
	width: 100%;
}



.mod-download-item{
	width: 45.83%;
}
.mod-download-item .code-item{
	float: left;
	width: 20.25%;
}
.mod-download-item .code-item .code-img{
	width: 100%;
	overflow: hidden;
	border-radius: 3px;
}
.mod-download-item .code-item .code-img img{
	width: 100%;
	display: block;
}
.mod-download-item .btn-warp{
	float: left;
	padding-top: 2.2%;
	width: 78.5%;
}
.mod-download-item .btn-warp .btn{
	float: left;
	margin-left: 7%;
	background: url(../images/hcq/btn-bg-160x68.png) no-repeat;
	background-size: contain;
	background-position: center;
	width: 43%;
	text-align: center;
	color: #fff;
	font-size: 0;
	padding: 6% 0;
	-webkit-transition: 600ms;
	-moz-transition: 600ms;
	-ms-transition: 600ms;
	-o-transition: 600ms;
	transition: 600ms;
}
.mod-download-item .btn-warp .btn:hover{
	background: url(../images/hcq/btn-bg-hover-160x68.png) no-repeat;
	background-size: contain;
	background-position: center;
	color: #c6a9d2;
	text-decoration: none;
	-webkit-transition: 600ms;
	-moz-transition: 600ms;
	-ms-transition: 600ms;
	-o-transition: 600ms;
	transition: 600ms;


}
.mod-download-item .btn-warp .btn .icon{
	display: inline-block;
	*+display: inline;
	*+zoom:1;
	_display: inline;
	_zoom:1;
	vertical-align: middle;
	font-size: 25px;
	margin-right: 6.25%;
}
.mod-download-item .btn-warp .btn span{
	display: inline-block;
	*+display: inline;
	*+zoom:1;
	_display: inline;
	_zoom:1;
	vertical-align: middle;
	font-size: 22px;
	cursor: pointer;
}

#fp-nav ul li a.active span,
.fp-slidesNav ul li a.active span {
    background: url(../images/hcq/page-icon-active-12x12.png) no-repeat;
}
#fp-nav ul li a span,
.fp-slidesNav ul li a span {
    background: url(../images/hcq/page-icon-12x12.png) no-repeat;
}

