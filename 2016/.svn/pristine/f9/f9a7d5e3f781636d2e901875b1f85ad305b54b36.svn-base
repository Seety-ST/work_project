/* common */

.animated {
  -webkit-animation-duration: 1s;
  animation-duration: 1s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}

/* mod btn */
.mod-download-item{
	width: 47.5%
}
.mod-download-item .code-item{
	float: left;
	width: 21.68%;
	overflow: hidden;
}
.mod-download-item .code-item img{
	width: 100%;
	border-radius: 5px;
}
.mod-download-item  .btn-warp{
	padding-top: 3.5%;
	float: left;
	width: 78.32%;
}
.mod-download-item  .btn-warp a{
	float: left;
	width: 41%;
	text-align: center;
	color: #fff;
	border: 2px solid #fff;
	border-radius: 5px;
	margin-left: 7%;
	padding: 4% 0;
	font-size: 22px;
	-moz-transition: all 600ms ease;
	-ms-transition: all 600ms ease;
	-o-transition: all 600ms ease;
	-webkit-transition: all 600ms ease;
	transition: all 600ms ease;
}
.mod-download-item .btn-warp a:hover{
	text-decoration: none;
	background-color: #fff;
}
.mod-download-item .btn-warp .icon{
	font-size: 25px;
	font-weight: normal;
}


/*  header */
.header{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	min-width: 1280px;
	padding-top: 4.167%;
	z-index: 3;
	background-color: #fff;
	border-bottom: 1px solid #efefef;
	animation-name: fadeInDown;
	-webkit-animation-name: fadeInDown;
	animation-duration: 2s;
	-webkit-animation-duration: 1s;
}
.header h1.logo{
	position: absolute;
	top: 0;
	left: 50%;
	width: 7.55%;
	margin-left: -73px;
}
.header h1.logo img{
	width: 100%;
}
.header .mod-nav-bar{
	position: absolute;
	top: 32%;
	right: 2.08%;
	width: 40%;
	font-size:16px;
}
.header .nav-btn-warp{
	float: right;
	width: 100%;
	text-align: right;
}
.header .nav-btn-warp .more-btn{
	float: right;
	width: 130px;
	-moz-transition: all 600ms ease;
	-ms-transition: all 600ms ease;
	-o-transition: all 600ms ease;
	-webkit-transition: all 600ms ease;
	transition: all 600ms ease;
	cursor: pointer;
	display: block;
	opacity: 1;
	margin-right: 0;
	filter:alpha(opacity=100);
}
.header .nav-btn-warp .more-btn:hover{
	opacity: 0.7;
	filter:alpha(opacity=70);
}
.header .nav-btn-warp .more-btn span{
	float: right;
	padding: 0 15px 0 0;
}
.header .nav-btn-warp .more-btn .icon{
	float: right;
	font-size: 17px;
	font-weight: 800;
}
.header .nav{
	display: none;
	opacity: 0;
	filter:alpha(opacity=0);
	float: right;
	margin-right: 0;
}
.header .nav li{
	float: left;
	padding-left: 32px;
}
.header .nav .n-line{
	display: block;
	margin: 0 auto;
	width: 100%;
	height: 2px;
	background:none; 
	margin-top: 4px;
}
.header .nav  li a:hover{
	text-decoration: none;
}
.header .nav  li a:hover .n-line{
	background-color: #171717; 
	animation:navcur 0.3s forwards linear;
	-webkit-animation:navcur 0.3s forwards linear;
}
.header .nav  li.active a .n-line,
.header .nav  li.active a:hover .n-line{
	background-color: #171717; 
	animation:navcur 0s forwards linear;
	-webkit-animation:navcur 0s forwards linear; 
}


/* footer */
.footer{ 
	background: #fff;
	width: 100%;
	min-width: 1280px;
	text-align: center;
	padding: 1.2% 0 1%;
	font-size: 12px;
	color: #adadad;
	border-top: 1px solid #f6f6f8;
	animation-name: fadeInUp;
	-webkit-animation-name: fadeInUp;
	-webkit-animation-duration: 1s;
	animation-duration: 1s;
	z-index: 3;

}
.footer .footer-nav{
	font-size: 14px;
	color: #8f8f8f;
	padding-bottom: 5px;
}
.footer .footer-nav a{
	margin: 0 34px;
	color: #8f8f8f;
}

/* animation */
@-webkit-keyframes fadeIn {
	from {
		opacity: 0;
	}

	to {
		opacity: 1;
	}
}

@keyframes fadeIn {
	from {
		opacity: 0;
	}

	to {
		opacity: 1;
	}
}
@-webkit-keyframes fadeInDown {
	from {
		opacity: 0;
		-webkit-transform: translate3d(0, -100%, 0);
		transform: translate3d(0, -100%, 0);
	}

	to {
		opacity: 1;
		-webkit-transform: none;
		transform: none;
	}
}
@keyframes fadeInDown {
	from {
		opacity: 0;
		-webkit-transform: translate3d(0, -100%, 0);
		transform: translate3d(0, -100%, 0);
	}

	to {
		opacity: 1;
		-webkit-transform: none;
		transform: none;
	}
}
@-webkit-keyframes fadeInUp {
	from {
		opacity: 0;
		-webkit-transform: translate3d(0, 100%, 0);
		transform: translate3d(0, 100%, 0);
	}

	to {
		opacity: 1;
		-webkit-transform: none;
		transform: none;
	}
}

@keyframes fadeInUp {
	from {
		opacity: 0;
		-webkit-transform: translate3d(0, 100%, 0);
		transform: translate3d(0, 100%, 0);
	}

	to {
		opacity: 1;
		-webkit-transform: none;
		transform: none;
	}
}
@-webkit-keyframes navcur 
{
	0%{
		transform:scale(0,0);
	}
	100%{
		transform:scale(1,1);
	}
}
@keyframes navcur 
{
	0%{
		transform:scale(0,0);
	}
	100%{
		transform:scale(1,1);
	}
}