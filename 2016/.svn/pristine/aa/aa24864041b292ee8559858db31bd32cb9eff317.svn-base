	<script type="text/javascript" src="js/jquery-1.11.0.min.js"></script>
	<script src="js/jquery.fullPage.min.js"></script>
	<script>
	useImg();
	function useImg()
	{
	    jQuery(function($){
	    				var arr = $('#mod-user-item ul li').toArray();
	    				var len = arr.length;
	    				var rand = parseInt(Math.random()*(len)); 
	    				$('#mod-user-item ul li').each(function(i){
	    						 $('#mod-user-item ul').append($('ul li').eq(rand));
	    						 rand = parseInt(Math.random()*(len)); 
	    				});   
	    });
	    setTimeout("useImg()", 2000);
	};	
	$(document).ready(function() {
		$(function(){
			$('#circle-page').fullpage({
				'navigation' : true,
				'scrollingSpeed' : 500,
				'continuousVertical' : false,
				'css3' : true
			});
		});
	});
	</script>
