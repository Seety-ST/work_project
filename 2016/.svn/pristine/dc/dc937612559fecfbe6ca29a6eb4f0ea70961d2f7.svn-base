/* ====================== 重置结束 ===================== */
* { -webkit-tap-highlight-color: transparent; -webkit-appearance: none; }
html { -ms-touch-action: none; }
body{
	background: #dd0e46 url(../images/bg-img-750x1152.jpg) center top no-repeat;
	background-size: 100% auto;
}
html, body { 
	font-family: 'Microsoft YaHei', 'Verdana', 'SimSun';
}
input,select{
	-webkit-appearance:none;
	appearance:none; 
	border-radius: 0; 
}
input[type="submit"],input[type="reset"],input[type="button"],input{-webkit-appearance:none;}






/* index */
.index-page{
	padding: 44.8% 0 0;
	width: 100%;
	overflow: hidden;
}
.index-page img{
	width: 100%;
	display: block;
}
.index-page h1.logo{
	display: block;
	width: 14.4%;
	position: absolute;
	top: 0;
	left: 50%;
	-webkit-transform: translate(-50%,0%);
}
.index-page .listItem{
	padding-bottom: 15.13%;
}
.index-page .lucky-item{
	width: 78.267%;
	margin: 0 auto;
	position: relative;
}
.index-page .lucky-item .monkey-icon{
	display: block;
	width: 34.582%;
	position: absolute;
	top: -5.6%;
	left: 50%;
	-webkit-transform: translate(-50%,-50%);
	z-index: 3;
}
/* 抽奖css */
.index-page .lucky-item .Disc{
	width: 100%; 
	text-align: center;
}
.index-page .lucky-item img.Needle{
	position: absolute;
	left: 50%;
	top: 50%;
	-webkit-transform: translate(-50%,-50%);
	width: 27.946%;
}
/* 抽奖css end */
.index-page  .gif-tips{
	background: url(../images/tips-img-511x107.png?v=1) center top no-repeat;
	background-size: 100% 100%;
	color: #fff;
	text-align: center;
	margin: 0.5% auto 0;
	width: 68%;
	padding: 3.5% 0 2.5%;
	font-size: 24px;
}
.index-page .download-link{
	text-align: center;
	padding-top: 5%;
}
.index-page .download-link a{
	color: #ffe400;
	text-decoration: underline;
}
.index-page .btn-item{
	padding: 8% 0 5%;
}
.index-page .btn-item a{
	float: left;
	width: 50%;
}
.index-page .btn-item a:active{
	opacity: 0.9
}
.index-page .gz-item{
	background: #c70015 url(../images/bg-img-750x1044.jpg) center top no-repeat;
	background-size: 100% auto;
	padding: 62% 5% 7%;
	color: #f7d9ae;
	font-size: 13px;
}
.index-page .gz-item li{
	width: 100%;
	display:-webkit-box;
}
.index-page .gz-item .num{
	display:block;
	background: url(../images/num-img-73x82.png) center top no-repeat;
	background-size: 100% 100%;
	width: 36.5px;
	height: 41px;
	line-height: 41px;
	text-align: center;
	font-size: 13.44px;
	font-style: normal;
}
.index-page .gz-item .txt{
	-webkit-box-flex:1;
	display: block;
	line-height: 23px;
	padding-top: 2.5%;
}
.index-page .gz-item .hd-li{
	font-size: 14px;
}
.index-page .gz-item .tips{
	text-align: center;
	-webkit-transform: scale(0.9);
	padding-top: 5%;
}

/* 弹出层 */
.fade{
	width: 100%;
	height: 100%;
	background: rgba(0,0,0,.7);
	position: fixed;
	top: 0;
	left: 0;
	z-index: 4;
}
/* 礼物弹出层 */
.pop-gift{
	width: 81.6%;
	background: url(../images/pop-bg-522x818.png) no-repeat;
	background-size: 100% auto;
	position: fixed;
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%,-50%);
	z-index: 5;
}
.pop-gift img{
	width: 100%;
	display: block;
}
.pop-gift .close{
	width: 9.2%;
	position: absolute;
	right: -6.9%;
	top: 17%;
}
.pop-gift .pop-con{
	padding: 45% 0 11%;
}
.pop-gift .pop-con .listItem{
	text-align: center;
	font-size: 12px;
	width: 100%;
	overflow: hidden;
}
.pop-gift .pop-con .listItem dt{
	font-size: 16px;
	color: #7a3900;
	font-weight: bold;
}
.pop-gift .pop-con .listItem dt .point{
	color: #ff2f30
}
.pop-gift .pop-con .listItem dd{
	color: #f16c46;
	-webkit-transform:scale(0.83);
}
.pop-gift .pop-con .gift-img{
	padding: 0 1.92% 20%;
}
.pop-gift .input-warp{
	width: 90.4%;
	padding: 0 4.8%;
}
.pop-gift .input-warp .input-item{
	float: left;
	width: 61%;
	position: relative;
}
.pop-gift .input-warp .input-item .input-txt{
	position: absolute;
	width: 70%;
	height: 100%;
	left: 0;
	top: 0;
	background: none;
	border: 0;
	padding: 0% 5% 0% 25%;
	color: #f16b46;
	font-family: 'Microsoft YaHei', 'Verdana', 'SimSun';
}
.pop-gift .input-warp .btn{
	width: 36.5%;
	float: right;
}
/* 机会用完 */
.pop-used{
	width: 89.867%;
	background: url(../images/pop-usedbg-674x97.png) no-repeat;
	background-size: 100% auto;
	position: fixed;
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%,-50%);
	z-index: 5;
}
.pop-used .close{
	width: 9.2%;
	position: absolute;
	right: 1%;
	top: -5%;
}
.pop-used img{
	width: 100%;
	display: block;
}
.pop-used .pop-con{
	padding: 39% 0 15%;
}
.pop-used .btn-item{
	width: 47.47%;
	margin: 0 auto;
}
.pop-used .btn-item .btn{
	display: block;
	margin-bottom: 10%;
}
.pop-used .btn-item .share-btn{
	position: relative;
	width: 100%;
}
.pop-used .btn-item .share-btn i.icon{
	position: absolute;
	top: -30%;
	left: -16%;
	width: 27.5%
}
.pop-used .btn-item .btn:active{
	opacity: 0.5
}
/* 分享弹出层 */
.pop-share{
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	z-index: 5;
}
.pop-share img{
	width: 100%;
	display: block;
}
.pop-share i.top-icon{
	position: absolute;
	top: 15%;
	right: 5%;
	width: 8.53%;
	-webkit-animation:imgcur 1.2s infinite linear ;
}
@-webkit-keyframes imgcur 
{
    0%{
            -webkit-transform: translate(0,-5px);
        }
        25%{
            -webkit-transform: translate(0,0);
        }
        50%{
            -webkit-transform: translate(0,-5px);
        }
        100%{
            -webkit-transform: translate(0,-5px);
        }
}
/* 文字弹出层 */
.pop-txt{
	width: 89.867%;
	background: url(../images/pop-txtbg-674x348.png?v=1) no-repeat;
	background-size: 100% 100%;
	position: fixed;
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%,-50%);
	z-index: 5;
}
.pop-txt .close{
	width: 9.2%;
	position: absolute;
	right: 2%;
	top: -5%;
}
.pop-txt img{
	width: 100%;
	display: block;
}
.pop-txt .pop-con{
	padding: 15% 0 10%;
	text-align: left;
	color: #793800;
	font-size: 20px;
}
.pop-txt .pop-con .txt-item{
	padding: 0 19% 15%;
	font-weight: bold;
	text-align: center;
}
.pop-txt .btn-item{
	width: 47.47%;
	margin: 0 auto;
}
.pop-txt .btn-item .btn{
	display: block;
	margin-bottom: 10%;
}
.pop-txt .btn-item .share-btn{
	position: relative;
	width: 100%;
}
.pop-txt .btn-item .share-btn i.icon{
	position: absolute;
	top: -30%;
	left: -16%;
	width: 27.5%
}
.pop-txt .btn-item .btn:active{
	opacity: 0.5
}








