/* 3图焦点切换 */
.mod-lookback-item{  
    padding:27px 0 0px; 
    position:relative;  
    width:1000px; 
    height: 582px;
}
#look-imglist{ 
    margin:0 auto;  
    width:1000px; 
    height:406px; 
    position:relative;
    overflow: hidden;
}
#look-imglist .look-item {  
    position:absolute;
    top:-1000px;
    left:-1000px;
    cursor:pointer; 
    filter:progid:DXImageTransform.Microsoft.gradient(enabled='true',startColorstr='#4F000000', endColorstr='#4F000000');
    background:rgba(0,0,0,0.31);  
}
#look-imglist a{ 
    text-decoration:none
}
#look-imglist .img-responsive { 
    border:0; 
    display:block;
}
.mod-lookback-item #prev,
.mod-lookback-item #next {
    z-index: 4; 
    overflow: hidden; 
    cursor: pointer; 
    display: block; 
    position: absolute; 
    width:36px; 
    height:66px; 
    top:194px; 
    background-repeat:no-repeat;
}
.mod-lookback-item #prev {  
    left:154px; 
    background-image: url(../images/ll-img-9.png); 
}
.mod-lookback-item #next { 
    right:154px; 
    background-image: url(../images/ll-img-10.png);
}
.mod-lookback-item .tracker-summation-container,
.mod-lookback-item .tracker-individual-container{
    display: none !important;
}

