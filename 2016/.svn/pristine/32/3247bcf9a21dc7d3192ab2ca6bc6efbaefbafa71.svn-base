	<script type="text/javascript" src="js/jquery.js"></script>
	<script>
	var city = new Array(); //缓存
	var dealer = new Array(); //缓存
	$('#province').on('change', function() {
		var prov = $('[name="province"]').val();
		if (city[prov] == undefined) {
			$.getJSON('ajaxAddr.php', {
				"prov": prov
			}, function(json) {
				var html = '';
				$.each(json, function(i, v) {
					html += '<option value="' + v.city + '">' + v.city + '</option>'
				});
				city[prov] = html;
				$('#city').html(html);
				$('#city').trigger('change');
			});
		} else {
			$('#city').html(city[prov]);
			$('#city').trigger('change');
		}
	});
	$('#city').on('change', function() {
		var c = $('[name="city"]').val();
		if (dealer[c] == undefined) {
			$.getJSON('ajaxAddr.php', {
				"city": c
			}, function(json) {
				var html = '';
				$.each(json, function(i, v) {
					html += '<option value="' + v.id + '">' + v.addr + '</option>'
				});
				dealer[c] = html;
				$('#dealer').html(html);
			});
		} else {
			$('#dealer').html(dealer[c]);
		}
	});
	$('#province').val('{province}');
	$('#province').trigger('change');

	function subForm() {
		if (!/^[\u4e00-\u9fa5]+$/i.test($('[name="name"]').val())) {
			alert('请填写真实姓名！');
			$('[name="name"]').focus();
			return false;
		}
		if (!/^[1][3-8][\d][\d][\d][\d][\d][\d][\d][\d][\d]+$/.test($('#phone').val())) {
			alert('请填写手机号码！');
			$('#phone').focus();
			return false;
		}
		$('[name="driveForm"]').submit();
	}

	function showItem(id, obj) {
		$('#table-con-items .table-item').hide();
		$('#table-item-'+id).show();
		$('#input-text').val($(obj).text());
		$('#select-pop').hide();
	}

	function selectPop() {
		$('#select-pop').show();
	}

	function addLike(id) {
		$.getJSON('ajaxLike.php', {
			"id": id
		}, function(json) {
			if (json.ok) {
				var v = parseInt($('#team-'+id).text());
				$('#team-'+id).text(v+1);
			} else {
				alert('操作失败！');
			}
		});
	}
	$("#nav-list").find('li').on('click', function(event) {
		$(this).addClass('cur').siblings().removeClass('cur');
		var index = $(this).index();
		

	});
	</script>

