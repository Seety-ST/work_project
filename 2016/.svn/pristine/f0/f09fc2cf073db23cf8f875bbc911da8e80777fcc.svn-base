	<script>
	useImg();
	function useImg()
	{
	    jQuery(function($){
	    				var arr = $('#mod-user-item ul li').toArray();
	    				var len = arr.length;
	    				var rand = parseInt(Math.random()*(len)); 
	    				$('#mod-user-item ul li').each(function(i){
	    						 $('#mod-user-item ul').append($('ul li').eq(rand));
	    						 rand = parseInt(Math.random()*(len)); 
	    				});   
	    });
	    setTimeout("useImg()", 2000);
	};	
	$(document).ready(function() {
		$(function(){
			$('#circle-page').fullpage({
				'navigation' : true,
				'scrollingSpeed' : 500,
				'continuousVertical' : false,
				'css3' : true
			});
		});
		$("#mod-nav-bar").on("click",function(){
			$("#mod-nav-bar").find("#header-more-btn").delay(100).animate({
				opacity: '0',
				marginRight: '2.5%'
			}).hide();
			$("#mod-nav-bar").find("#nav").delay(100).animate({
				opacity: '1',
				marginRight: '0'										
			}).show();
		});
		$("#mod-nav-bar").on("mouseleave",function(){
			$("#mod-nav-bar").find("#header-more-btn").delay(100).animate({
				opacity: '1',
				marginRight: '0'
			}).show();
			$("#mod-nav-bar").find("#nav").delay(100).animate({
				opacity: '0',
				marginRight: '2.5%'							
			}).hide();
		});
	});
