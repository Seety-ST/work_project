						<div class="com-item">
							<div class="input-wrap">
								<div class="input-item clearfix">
									<h4 class="title">姓 名</h4>
									<input type="text" class="input-txt" />
								</div>
								<div class="input-item clearfix">
									<h4 class="title">手 机</h4>
									<input type="text" class="input-txt" />
								</div>
								<p class="tips"></p>
								<div class="btn-item">
									<a href="#" class="btn"></a>
								</div>
							</div>
						</div>
					</div>
				</div>
				<!-- 美人积分-参加过活动 -->
				<div class="form-item mrjf-item hadjoin-item" style="display:none">
					<div class="listItem"></div>
