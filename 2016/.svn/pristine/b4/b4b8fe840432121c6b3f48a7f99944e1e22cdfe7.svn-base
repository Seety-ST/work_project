        });
    });
    function showDiv(id, i) {
        $('#fade').show();
        $(id).show();
        var swiper1 = new Swiper('.swiper-container'+i, {
            pagination: '.swiper-pagination'+i,
            paginationClickable: true,
            // Disable preloading of all images
            preloadImages: false,
            // Enable lazy loading
            lazyLoading: true
        });
    }
