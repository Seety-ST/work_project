/* 首页 */
.index-page{ 
    position: relative; 
    overflow: hidden;
    padding-bottom: 30%;

}
.index-page .bg-item{
    background: url(../images-v4/ll-img-22.png);
    background-size: 100% auto;
    background-repeat: no-repeat;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 150%;
    -webkit-animation:joinbg 2s infinite linear ;
}
.index-page .info-btn{
    position: relative;
    padding-top: 2%;
    z-index: 2
}
.index-page .info-btn a{ 
    float: right;
    padding-right: 2%;
    width: 34.375%;
    -webkit-animation:infobtna 2s infinite linear ;
}
.index-page .info-btn a:active{
    -webkit-animation:infobtna 0.5s infinite linear ;
}
@-webkit-keyframes infobtna 
{
    0%{
        padding-right: 2%; 
    }
    50%{
        padding-right: 0%; 
    }
    100%{
       padding-right: 2%; 
    }
}
.index-page .info-btn img{ 
    width: 100%;
    display: block;
}
.index-page .item{ 
    padding: 74.68% 0 0 40.3125%; 
    position: relative; 
}
.index-page .btn-item{
    width: 100%;
}
.index-page .btn-item .c-icon{ 
    display: block; 
    width: 83.2%;
}
.index-page .btn-item .c-icon img{ 
    width:100%;
    display: block;
    -webkit-animation:bpicon 3.9s infinite;
    -webkit-animation-timing-function: step-start;
 }
@-webkit-keyframes bpicon 
{
    0%, 7.7% {
     background-image: url(../images-v4/index-btn/1.png);
     background-size: 100% 100%;
    }
    10.26% {
     background-image: url(../images-v4/index-btn/2.png);
     background-size: 100% 100%;
    }
    15.39% {
     background-image: url(../images-v4/index-btn/3.png);
     background-size: 100% 100%;
    }
    17.95% {
     background-image: url(../images-v4/index-btn/4.png);
     background-size: 100% 100%;
    }
    20.51% {
     background-image: url(../images-v4/index-btn/5.png);
     background-size: 100% 100%;
    }
    23.07% {
     background-image: url(../images-v4/index-btn/6.png);
     background-size: 100% 100%;
    }
    100% {
    background-image: url(../images-v4/index-btn/7.png);
    background-size: 100% 100%;
    }
}
.index-page .btn-item:active a img{ 
    opacity: 0.9
}

/* 活动说明页 */
.activity-page{ 
    width: 100%; 
    background: url(../images-v4/bg-img/2.jpg);
    background-size: 100% auto;
    background-repeat: no-repeat;
    position: relative;
}
.activity-page img{ 
    width: 100%;
    display: block;
}
.activity-page .bg-item{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    overflow: hidden;
    background: url(../images-v4/ll-img-4.png);
    background-size: 100% auto;
    background-repeat: no-repeat;
    -webkit-animation:joinbg 2s infinite linear ;
}
.activity-page .step-item{
    padding: 58.75% 0 100%;
}
.activity-page .step-item .item-list{
    position: relative;
    padding: 0 2.8125%;
    width: 94.375%;
}
.activity-page .step-item .item-list .item{
    position: absolute;
    width: 100%;
    top: 0;
}
.activity-page .step-item .item-list .s-bg{
    opacity: 0;
}
.activity-page .step-item .item-list .item-1{
    left: 0;
    z-index: 1;
}
.activity-page .step-item .item-list .item-1 .s-bg{
    -webkit-animation:stepimg 0.5s linear forwards;
    -webkit-animation-delay: 0.2s;
}
.activity-page .step-item .item-list .item-2{
    right: 0;
    z-index: 2;
}
.activity-page .step-item .item-list .item-2 .s-bg{
    -webkit-animation:stepimg 0.5s linear forwards;
    -webkit-animation-delay: 0.7s;
}
.activity-page .step-item .item-list .item-2 .zt-icon{
    position: absolute;
    top: -0.4%;
    right: 34.53125%;
    width: 22.34375%;
    -webkit-animation:ztIcon 2s infinite linear ;
    -webkit-animation-delay: 1.2s;
    opacity: 0;
}
@-webkit-keyframes ztIcon 
{
    0%{
            -webkit-transform: rotate(0deg);
            -webkit-transform-origin: top center;
            opacity: 1;
        }
        25%{
            -webkit-transform: rotate(-5deg);
            -webkit-transform-origin: top center;
            opacity: 1;
        }
        50%{
            -webkit-transform: rotate(0deg);
            -webkit-transform-origin: top center;
            opacity: 1;
        }
        75%{
            -webkit-transform: rotate(5deg);
            -webkit-transform-origin: top center;
            opacity: 1;
        }
        100%{
            -webkit-transform: rotate(0deg);
            -webkit-transform-origin: top center;
            opacity: 1;
        }
}
.activity-page .step-item .item-list .item-3{
    left: 0;
    z-index: 3;
    padding-top: 50%;
}
.activity-page .step-item .item-list .item-3 .s-bg{
    -webkit-animation:stepimg 0.5s linear forwards;
    -webkit-animation-delay: 1.2s;
}
.activity-page .step-item .item-list .item-4{
    right: 0;
    z-index: 4;
    padding-top: 50%;
}
.activity-page .step-item .item-list .item-4 .s-bg{
    -webkit-animation:stepimg 0.5s linear forwards;
    -webkit-animation-delay: 1.7s;
}
@-webkit-keyframes stepimg 
{
    0%{
        opacity: 0;

    }
    100%{
       opacity: 1 ;
    }
}
.activity-page .download-btn{
    position: absolute;
    top: 77.5%;
    right: 12%;
    width: 18%;
    -webkit-animation:infoDown 1s infinite linear ;
    -webkit-animation-delay: 1.8s;
    opacity: 0;
}
@-webkit-keyframes infoDown 
{
    0%{
            -webkit-transform: rotate(0deg);
            opacity: 1;
        }
        25%{
            -webkit-transform: rotate(10deg);
            opacity: 1;
        }
        50%{
            -webkit-transform: rotate(0deg);
            opacity: 1;
        }
        75%{
            -webkit-transform: rotate(-10deg);
            opacity: 1;
        }
        100%{
            -webkit-transform: rotate(0deg);
            opacity: 1;
        }
}
.activity-page .download-btn:active{
    opacity: 0.8;
}
.activity-page .download-btn img{
    width: 100%;
    display: block;
}

