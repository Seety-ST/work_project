/** 清除内外边距 **/
body, h1, h2, h3, h4, h5, h6, hr, p, blockquote, /* structural elements 结构元素 */
dl, dt, dd, ul, ol, li, /* list elements 列表元素 */
pre, /* text formatting elements 文本格式元素 */
form, fieldset, legend, button, input, textarea, /* form elements 表单元素 */
th, td, /* table elements 表格元素 */
img/* img elements 图片元素 */ { border: medium none; margin: 0; padding: 0; }
/** 设置默认字体 **/
body, button, input, select, textarea { font: 12px/1.5  "SimSun", Tahoma, Arial, sans-serif }
input, select, form img, button { vertical-align: middle; }
h1, h2, h3, h4, h5, h6 { font-size: 100%; }
em { font-style: normal; }
/** 重置列表元素 **/
ul, ol { list-style: none; }
/** 重置超链接元素 **/
a { text-decoration: none; color: #fff; }
a:hover { text-decoration: none }
/** 重置图片元素 **/
img { border: none; }
/** 重置表格元素 **/
table { border-collapse: collapse; border-spacing: 0; }
/** 清楚浮动 **/
.clearfix:after { visibility: hidden; display: block; font-size: 0; content: " "; clear: both; height: 0; }
* html .clearfix { zoom: 1; }
/* IE6 */
*:first-child+html .clearfix { zoom: 1; }
/* IE7 */
textarea, input { resize: none }
a, textarea, input, button { outline: none }
.font_wryh { font-family: '微软雅黑', '黑体', '宋体'; }
/** position **/
.re { position: relative; z-index: 1 }
.fr { float: right; }
.fl { float: left; }
.frdi { float: right; display: inline }
.fldi { float: left; display: inline }
/* ====================== 重置结束 ===================== */
* { -webkit-tap-highlight-color: transparent; -webkit-appearance: none; }
html { 
	-ms-touch-action: none;
	font-size: 62.5%;
}
body{
	background: #a4e0ff url(../images/bg-img-640x3140.jpg) center top no-repeat;
	background-size: 100% auto;
}
html, body { 
	font-family: 'Microsoft YaHei', 'Verdana', 'SimSun';
}
input,select,button{
	-webkit-appearance:none;
	appearance:none; 
	border-radius: 0; 
}
input[type="submit"],input[type="reset"],input[type="button"],input{-webkit-appearance:none;}

/* lazyload css */
.lazyload,
.lazyloading {
    opacity: 0;
}
.lazyloaded {
    opacity: 1;
    transition: opacity 300ms;
}

.lazyload {}

.lazyloading {
  min-height: 60px;
}

.teaser.lazyload {
	opacity: 0;
	transform: scale(0.95);
}

.teaser.lazyloaded {
	opacity: 1;
	transform: scale(1);
	transition: all 600ms;
}

/* 公共 */
.fade{
	background: rgba(0,0,0,.9);
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
}
.pop-bigimg{
	position: fixed;
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%,-50%);
	width: 75%;
}
.pop-bigimg img{
	width: 100%;
	display: block;
}

/* content */
.honor-page{
	padding: 100% 0 10%;
	color: #fff;
}
.honor-page img{
	width: 100%;
	display: block;
}
.honor-page h1.logo{
	position: absolute;
	top: 0;
	left: 4.6875%;
	width: 30.46875%;
}
.honor-page video{
	width: 90.625%;
	margin: 0 auto;
	display: block;
}
.honor-page .product-list{
	padding: 8.276% 4.6875% 0;
	text-align: center;
}
.honor-page .product-list .listItem{
	width: 100%;
	color: #01b4e5;
	font-size: 1.6rem;
	font-weight: bold;
}
.honor-page .product-list .tab-nav{
	font-size: 1.2rem;
	padding-top: 1%;
}
.honor-page .product-list .tab-nav a{
	margin: 0 3.75%;
}
.honor-page .product-list .tab-nav a.cur{
	color: #01b4e5;
}
.honor-page .product-list .list li{
	float: left;
	width: 48.275%;
	padding: 3.45% 1.725% 0 0;
}
.honor-page .product-list .list li:nth-of-type(2n){
	padding: 3.45% 0 0 1.725%;
}
.honor-page .product-list .btn-item{
	background: #01b4e5;
	padding: 3.5% 0;
	font-size: 1.2rem;
	line-height: 1.2;
}
.honor-page .product-list .btn-item:active{
	opacity: 0.8
}
.honor-page .product-list .icon{
	display: inline-block;
	vertical-align: middle;
	font-size: 0;
	height: 0;
	background-repeat: no-repeat;
	background-size: 100% 100%;
	margin-right: 2%;
}
.honor-page .product-list .like-icon{
	width: 8.21%;
	padding-top: 7.14%;
	background-image: url(../images/like-icon-23x20.png);
}
.honor-page .product-list .page{
	padding-top: 8.62%;
	font-size: 1.2rem;
}
.honor-page .product-list .page a{
	margin: 0 2.58%;
}
.honor-page .product-list .page a.click{
	color: #00c8ff;
}
.honor-page .product-list .page a:active{
	opacity: 0.8;
}


