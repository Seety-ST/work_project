/* ====================== 重置结束 ===================== */
* { -webkit-tap-highlight-color: transparent; -webkit-appearance: none; }
html { 
	-ms-touch-action: none;
	font-size: 62.5%;
}
body{
	background: #fff url(../images/baby/body-bg-750x3824.jpg) center top no-repeat;
	background-size: 100% auto;
	font-size: 1.6rem;
}
html, body { 
	font-family: 'Microsoft YaHei', 'Verdana', 'SimSun';
}

.footer{
	color: #2eb0bb
}
.footer a{
	color: #2eb0bb;
	padding: 0 5%;
}

.mod-down-btn .btn{
	box-sizing: box;
	border: 1px solid #fff;
	border-radius: 32px;
	display: block;
	width: 40%;
	padding: 1.65% 0;
	margin: 0 auto;
	font-size: 1.4rem;
	line-height: 1.2;
	-webkit-transition: 300ms;
}
.mod-down-btn .btn em{
	font-style: normal;
	display: inline-block;
	vertical-align: middle;
}
.mod-down-btn .btn .icon{
	display: inline-block;
	vertical-align: middle;
	font-size: 1.7rem;
	font-weight: normal;
	margin-right: 4.76%;
}
.mod-down-btn .btn:active{
	background: #fff;
	color: #2eb0bb;
	-webkit-transition: 300ms;
}


/* 内容 */
.baby-page{
	text-align: center;
	color: #18cea7;
}
.baby-page img{
	width: 100%;
	display: block;

}
.baby-page .info-item{
	background: url(../images/baby/header-bg-750x630.png) center top no-repeat;
	background-size: 100% auto;
	padding: 83.9% 0 0;
	position: relative;
}
.baby-page .info-item .mod-down-btn{
	position: absolute;
	top: 35.5%;
	left: 0%;
	width: 100%;
}
.baby-page .info-item .img-item{
	background: url(../images/baby/bg-750x886.jpg) no-repeat;
	background-size: 100% auto;
	width: 100%;
	padding-top: 118.1333%;
}
.baby-page .con-item{
	padding-bottom: 8.6%;
}
.baby-page .con-item .img-warp{
	padding-bottom: 7.6%;
}
.baby-page .con-item .mod-down-btn .btn{
	border: 1px solid #2eb0bb;
	background: #2eb0bb
}
.baby-page .con-item .mod-down-btn .btn:active{
	background: #fff;
	color: #2eb0bb;
}




