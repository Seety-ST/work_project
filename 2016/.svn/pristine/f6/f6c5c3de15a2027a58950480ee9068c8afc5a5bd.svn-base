	$('#province').on('change', function() {
		var prov = $('[name="province"]').val();
		if (city[prov] == undefined) {
			$.getJSON('ajaxAddr.php', {
				"prov": prov
			}, function(json) {
				var html = '';
				$.each(json, function(i, v) {
					html += '<option value="' + v.city + '">' + v.city + '</option>'
				});
				city[prov] = html;
				$('#city').html(html);
				$('#city').trigger('change');
			});
		} else {
			$('#city').html(city[prov]);
			$('#city').trigger('change');
		}
	});
	$('#city').on('change', function() {
		var c = $('[name="city"]').val();
		if (dealer[c] == undefined) {
			$.getJSON('ajaxAddr.php', {
				"city": c
			}, function(json) {
				var html = '';
				$.each(json, function(i, v) {
					html += '<option value="' + v.id + '">' + v.addr + '</option>'
				});
				dealer[c] = html;
				$('#dealer').html(html);
			});
		} else {
			$('#dealer').html(dealer[c]);
		}
	});
	$('#province').val('{province}');
