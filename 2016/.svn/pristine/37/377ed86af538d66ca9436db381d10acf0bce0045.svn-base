/** 清除内外边距 **/
body, h1, h2, h3, h4, h5, h6, hr, p, blockquote, /* structural elements 结构元素 */
dl, dt, dd, ul, ol, li, /* list elements 列表元素 */
pre, /* text formatting elements 文本格式元素 */
form, fieldset, legend, button, input, textarea, /* form elements 表单元素 */
th, td, /* table elements 表格元素 */
img/* img elements 图片元素 */ { border: medium none; margin: 0; padding: 0; }
/** 设置默认字体 **/
body, button, input, select, textarea { font: 12px/1.5  "SimSun", Tahoma, Arial, sans-serif }
input, select, form img, button { vertical-align: middle; }
h1, h2, h3, h4, h5, h6 { font-size: 100%; }
em { font-style: normal; }
/** 重置列表元素 **/
ul, ol { list-style: none; }
/** 重置超链接元素 **/
a { text-decoration: none; color: #333; }
a:hover { text-decoration: none; }
/** 重置图片元素 **/
img { border: none; }
/** 重置表格元素 **/
table { border-collapse: collapse; border-spacing: 0; }
/** 清楚浮动 **/
.clearfix:after { visibility: hidden; display: block; font-size: 0; content: " "; clear: both; height: 0; }
* html .clearfix { zoom: 1; }
/* IE6 */
*:first-child+html .clearfix { zoom: 1; }
/* IE7 */
textarea, input { resize: none }
a, textarea, input, button { outline: none }
.clear { clear: both; font-size: 0; line-height: 0; height: 0; overflow: hidden; }
.coz { clear: both; overflow: hidden; zoom: 1 }
/*** font ***/
.word_break { white-space: normal; word-wrap: break-word; word-break: break-all; }
.f14 { font-size: 14px; }
.fb { font-weight: bold; }
.db { display: block }
.dn { display: none }
.di { display: inline }
.tc { text-align: center; }
.vam { vertical-align: middle; }
.font_wryh { font-family: '微软雅黑', '黑体', '宋体'; }
/** position **/
.re { position: relative; z-index: 1 }
.fr { float: right; }
.fl { float: left; }
.frdi { float: right; display: inline }
.fldi { float: left; display: inline }
/* ====================== 重置结束 ===================== */
* { -webkit-tap-highlight-color: transparent; -webkit-appearance: none; }
html, body { 
	font-family: 'Microsoft YaHei', 'Verdana', 'SimSun'; 
}
html { 
	-ms-touch-action: none; 
	font-size: 62.5%; 
}
input,select,button{
	-webkit-appearance:none;
	appearance:none; 
	border-radius: 0; 
}

input[type="submit"],
input[type="reset"],
input[type="button"],
input{
	-webkit-appearance:none;
}

body { 
	background: #f0f0f0;
	font-size: 1.6rem;
}
em{
	font-style: normal;
}

/* 公共 */

.mod-tool-bar{
	background: rgba(255,255,255,.96);
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
}
.mod-tool-bar li{
	float: left;
	width: 50%;
	color: #fff;
	background: url(../images/beauty/bar-line-1x81.png) right center no-repeat;
	background-size: auto 100%;
	text-align: center;
	color: #18cea7;
	-webkit-transition: all 600ms ease;
}
.mod-tool-bar li a{
	display: block;
	width: 100%;
	text-align: center;
	font-size: 1.6rem;
	color: #18cea7;
	padding: 3.7% 0;
}
.mod-tool-bar li a:active{
	opacity: 0.4;
}
.mod-tool-bar li.no-click a,
.mod-tool-bar li.no-click a:active{
	opacity: 0.2;
}
.mod-tool-bar li i.icon{
	display: inline-block;
	vertical-align: middle;
	width: 9.6%;
	height: 0;
	padding-top: 9.6%;
	margin-right: 3.2%;
	background-repeat: no-repeat;
	background-size: 100% 100%;
}
.mod-tool-bar li .choose-icon{
	background-image: url(../images/beauty/choose-icon-34x34.png);
}
.mod-tool-bar li .set-icon{
	background-image: url(../images/beauty/set-icon-36x36.png);
}
.mod-tool-bar  li em{
	display: inline-block;
	vertical-align: middle;
}

.mod-tips-item{
	position: absolute;
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%,-50%); 
	width: 100%;
	color: #999;
	font-size: 1.6rem;
	line-height: 1.5;
	text-align: center;
}
.mod-tips-item .mod-icon{
	display: block;
	margin: 0 auto;
	width: 21.33%;
	height: 0;
	padding-top: 21.33%;
	background-repeat: no-repeat;
	background-size: 100% 100%;
	margin-bottom: 4%;
}
.no-wifi-item .nowifi-icon{
	background-image: url(../images/beauty/wifi-icon-160x160.png);
}
.no-pic-item .nopic-icon{
	background-image: url(../images/beauty/cloudpic-icon-160x160.png);
}

.mod-upload-btn{
	position: fixed;
	bottom: 8%;
	left: 50%;
	width: 18.67%;
	text-align: center;
	-webkit-transform: translate(-50%,0%); 
}
.mod-upload-btn .btn{
	display:block;
	width: 100%;
	padding: 20% 0 12%;
	border-radius: 50%;
	background: #27d3ae;
	color: #fff;
	font-size: 1.4rem;
	-webkit-filter: drop-shadow(2px 1px 0px rgba(0,0,0,.1));

}
.mod-upload-btn .btn:active{
	opacity: 0.4;
}
.no-click-upload-btn .btn,
.no-click-upload-btn .btn:active{
	opacity: 0.2;
}
.mod-upload-btn .upload-btn{
	display: block;
	width: 50%;
	height: 0;
	padding-top: 34.3%;
	background: url(../images/beauty/upload-icon-70x48.png);
	background-size: 100% 100%;
	margin: 0 auto 3.73%;
}
.mod-upload-btn .btn em{
	display: block;
	margin: 0 auto;
	-webkit-transform:scale(0.8533);
}

.fade{
    background: rgba(0,0,0,.3);
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 4;
}

/* tips pop */
.pop-tipsItem{
    width: 70%;
    position: fixed;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%,-50%); 
    z-index: 5;
    text-align: center;
}
.pop-tipsItem .text-item{
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    background: #f8f9f9;
    background-image: -webkit-gradient(linear,0% 0%, 0% 100%, from(#faf9f9), to(#f8f9f9));
    padding: 8% 7%;
    margin-bottom: 1px;
    font-size: 1.3rem;
}
.pop-tipsItem .btn-item{
    border-bottom-left-radius: 10px;
    border-bottom-right-radius: 10px;
    background: #f9f9f9;
    background-image: -webkit-gradient(linear,0% 0%, 0% 100%, from(#F9F9F9), to(#F9F9F9), color-stop(0.5,#F9FAF9));
    font-size: 1.5rem;
}
.pop-tipsItem .btn-item .area-btn{
    display: block;
    padding: 4% 5%;
    color: #0982ff;
}
.pop-tipsItem .btn-item .area-btn:active{
    opacity: 0.65;
}


/* content */
.cloud-page .content{
	padding: 10.67% 0 5%;
	font-size: 1.6rem;
}
.cloud-imgList-item .item dt.date-item{
	color: #333;
	font-size: 1.733rem;
	padding: 4.3% 2.67% 2%;
	line-height: 1.2;
}
.cloud-imgList-item .item .list{
	width: 100%;
}
.cloud-imgList-item .item .list li{
	width: 32.8%;
	float: left;
	padding: 0 0.8% 0.8% 0;
}
.cloud-imgList-item .item .list li:nth-of-type(3n){
	padding-right: 0;
}
.cloud-imgList-item .item .list li img{
	max-width: 100%;
	display: block;
}










/* ---分辨率--- */
@media screen and (max-width: 768px) and (max-height: 1024px) {
	html { 
		font-size: 128%; 
	} 
}
@media screen and (max-width: 621px) and (max-height: 1044px) {
	html { 
		font-size: 103.5%; 
	} 
}
@media screen and (max-width: 540px) and (max-height: 960px) {
	html { 
		font-size: 90%; 
	}  
}
@media screen and (max-width: 414px) and (max-height: 736px){
	html { 
		font-size: 69%; 
	}  
}
@media screen and (max-width: 375px) and (max-height: 627px) {
	html { 
		font-size: 62.5%; 
	} 
}
@media screen and (max-width: 360px) and (max-height: 640px) {
	html { 
		font-size: 60%; 
	} 
}
@media screen and (max-width: 320px) and (max-height: 568px) {
	html { 
		font-size: 53.33%; 
	}
}







