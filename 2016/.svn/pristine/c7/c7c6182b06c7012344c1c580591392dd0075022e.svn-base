.page { 
	background-repeat: no-repeat;
}
.contain { 
	position: relative; 
	width: 100%;
	height: 100%;

}
.
.page .con-item{
	width: 1024px;
	margin: 0 auto;
}

#navBar { 
	z-index: 3; 
	position: absolute; 
	top: 40%; 
	right: 24px;
}
#navBar .active { 
	background: url(../images/beauty/page-icon-active-12x12.png) no-repeat;
}
#navBar li { 
	cursor: pointer; 
	margin-bottom: 16px; 
	transition: all .7s ease; 
	line-height: 0px; 
	font-size: 0;
	text-align: center; 
	width: 12px; 
	height: 12px;
	background: url(../images/beauty/page-icon-12x12.png) no-repeat;
}


.mod-logo{
	width: 8.125%;
	position: fixed;
	top: 0;
	left: 5.5208%;
	z-index: 9;
}
.mod-logo img{
	display: block;
	width: 100%;
}


.mod-download-item{
	width: 386px;
}
.mod-download-item .code-item{
	float: left;
	width: 132px;
}
.mod-download-item .code-item .code-img{
	width: 132px;
	height: 132px;
	overflow: hidden;
	background: #fff;
}
.mod-download-item .code-item .code-img img{
	width: 132px;
}
.mod-download-item .btn-warp{
	float: left;
	width: 231px;
	padding-left: 23px;
}
.mod-download-item .btn-warp .btn{
	display: block;
	margin-bottom: 12px;
	background: #18cea7;
	width: 231px;
	height: 60px;
	text-align: center;
	color: #fff;
	font-size: 26px;
	line-height: 60px;
	-webkit-transition: 600ms;
	-moz-transition: 600ms;
	-ms-transition: 600ms;
	-o-transition: 600ms;
	transition: 600ms;
}
.mod-download-item .btn-warp .btn:hover{
	background: #13b08e;
	text-decoration: none;
}
.mod-download-item .btn-warp .btn .icon{
	font-size: 35px;
	float: left;
	padding: 0 13px;
}

.mod-download-item .btn-warp .btn span{
	float: left;
	width: 168px;
	border-left: 1px solid #fff;
	height: 60px;
	line-height: 60px;
	cursor: pointer;
}

#jpdown-page{
	min-width: 1024px;
}
