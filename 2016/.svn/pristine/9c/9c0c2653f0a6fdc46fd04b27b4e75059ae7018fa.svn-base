	<script>
		function tab(name,num,end)
		{
				for(i=1;i<=end;i++)
				{
						var d=document.getElementById(name+i);
						var a=document.getElementById("c-"+name+i);
						d.className=i==num?"cur":"";
						a.style.display=i==num?"block":"none"
				}
		}
		$(document).ready(function(){
			$('#navItem li').hover(function(){
				if($(this).hasClass('item')){
					$(this).removeClass('item');
					$(this).addClass('cur');
				}else{
					$(this).removeClass('cur');
					$(this).addClass('item');
				}
			});
		});
	</script>
