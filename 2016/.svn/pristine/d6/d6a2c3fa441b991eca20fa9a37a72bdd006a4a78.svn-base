/* ====================== 重置结束 ===================== */
* { -webkit-tap-highlight-color: transparent; -webkit-appearance: none; }
html { 
	-ms-touch-action: none;
	font-size: 62.5%;
}
body{
	background: #fff;
}
html, body { 
	font-family: 'Microsoft YaHei', 'Verdana', 'SimSun';
}
input,select{
	-webkit-appearance:none;
	appearance:none; 
	border-radius: 0; 
}
input[type="submit"],input[type="reset"],input[type="button"],input{-webkit-appearance:none;}

/* lazyload css */
.lazyload,
.lazyloading {
    opacity: 0;
}
.lazyloaded {
    opacity: 1;
    transition: opacity 300ms;
}

.lazyload {
  background: #fff;
}

.lazyloading {
  background: #fff;
  min-height: 60px;
}

.teaser.lazyload {
	opacity: 0;
	transform: scale(0.95);
}

.teaser.lazyloaded {
	opacity: 1;
	transform: scale(1);
	transition: all 600ms;
}

/* 下载浮动层 */
.f-download-btn{
	position: fixed;
	bottom: 0;
	left: 0;
	width: 100%;
	height: 0;
	padding-top: 9.867%;
	background: rgba(0,0,0,.7) url(../images/download-bg-750x74.png) center no-repeat;
	background-size: 100% auto;
}
.f-download-btn .icon-area{
	position: absolute;
	right: 0;
	top: 0;
	padding: 0 4%;
	width: 4%;
	height: 100%;
	display: block;
}
.f-download-btn .icon-area:active{
	opacity: 0.8
}
.f-download-btn .icon-area .close-icon{
	display: block;
	background: url(../images/close-icon-30x30.png) center no-repeat;
	background-size: 100% auto;
	width: 100%;
	height: 100%;
}
.f-download-btn .btn-item{
	position: absolute;
	top: 16%;
	right: 12%;
	display: block;
	width: 17.5%;
	height: 0;
	padding-top: 38.17%;
	background-image: url(../images/download-btn-131x50.png);
	background-repeat: no-repeat;
	background-size: 100% auto;
}
.f-download-btn .btn-item:active{
	background-image: url(../images/download-btn-active-131x50.png);
}

