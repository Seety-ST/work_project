.page .con-item{
	width: 1024px;
	margin: 0 auto;
}

#navBar { 
	z-index: 3; 
	position: absolute; 
	top: 40%; 
	right: 24px;
}
#navBar .active { 
	background: url(../images/yinxiang/page-icon-active-12x12.png) no-repeat;
}
#navBar li { 
	cursor: pointer; 
	margin-bottom: 16px; 
	transition: all .7s ease; 
	line-height: 0px; 
	font-size: 0;
	text-align: center; 
	width: 12px; 
	height: 12px;
	background: url(../images/yinxiang/page-icon-12x12.png) no-repeat;
}


.mod-logo{
	width: 12.29167%;
	position: fixed;
	top: 0;
	left: 5.729167%;
	z-index: 9;
}
.mod-logo img{
	display: block;
	width: 100%;
}


.mod-download-item{
	width: 100%;
}
.mod-download-item .code-item{
	float: left;
	width: 40.37%;
}
.mod-download-item .code-item .code-img{
	width: 100%;
	overflow: hidden;
}
.mod-download-item .code-item .code-img img{
	width: 100%;
}
.mod-download-item .btn-warp{
	float: left;
	width: 55.19%;
	padding-left: 4.44%;
}
.mod-download-item .btn-warp .btn{
	float: left;
	margin: 0.03rem 0 0.12rem;
	border: 1px solid #171717;
	width: 100%;
	text-align: center;
	color: #171717;
	font-size: 0.22rem;
	height: 0.41rem;
	line-height: 0.41rem;
	-webkit-transition: 600ms;
	-moz-transition: 600ms;
	-ms-transition: 600ms;
	-o-transition: 600ms;
	transition: 600ms;
}
.mod-download-item .btn-warp .btn:hover{
	background: #171717;
	color: #fff;
	text-decoration: none;
}
.mod-download-item .btn-warp .btn .icon{
	font-size: 0.25rem;
	font-weight: normal;
}


