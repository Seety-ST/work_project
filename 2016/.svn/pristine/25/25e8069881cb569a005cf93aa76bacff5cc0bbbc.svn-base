* { -webkit-tap-highlight-color: transparent; -webkit-appearance: none; }

html, body { 
	font-family: 'Microsoft YaHei', 'Verdana', 'SimSun'; 
}
html { 
	-ms-touch-action: none; 
	font-size: 62.5%; 
}
input,select,button{
	-webkit-appearance:none;
	appearance:none; 
	border-radius: 0; 
}

input[type="submit"],
input[type="reset"],
input[type="button"],
input{
	-webkit-appearance:none;
}

body{
	background:#eceef3 url(../images/bg-img.jpg?v=1) center top no-repeat;
	background-size: 100% auto;
	width: 100%;
	height: 100%;
}

.volvo-page{ 
	color: #171717; 
	padding-bottom: 5%;
}
.volvo-page img{ 
	width: 100%; 
	display: block;
}
.volvo-page .path1{ 
	position: relative; 
}
.volvo-page .path1 .bg-item{

}
.volvo-page .path1 .logo{ 
	width: 12.65625%; 
	position: absolute; 
	top: 4.0625%; 
	right: 3.4375%;
}
.volvo-page .path1 .btn-item{ 
	width: 9.21875%; 
	position: absolute;
	right: 0;
	top: 18%;
	z-index: 3;
	-webkit-animation:imgcur 1.2s infinite linear ;
}
@-webkit-keyframes imgcur 
{
	0%{
	    -webkit-transform: translate(0,-3px);
	}
	25%{
	    -webkit-transform: translate(0,0);
	}
	50%{
	    -webkit-transform: translate(0,-3px);
	}
	100%{
	    -webkit-transform: translate(0,-3px);
	}
}
.volvo-page .path1 .btn-item img:active{ 
	opacity: 0.9
}
.volvo-page .path2{ 
	padding: 0 0 12.65625%;
}
.volvo-page .path2 .fromList{ 
}
.volvo-page .path2 .fromList .item{ 
	padding:0 4.6875% 3%;

}
.volvo-page .path2 .fromList .input-txt{ 
	width: 72%; 
	box-sizing:border-box;
	border: 1px solid #abafb0; 
	background: none;
	padding: 1.7% 5%; 
	color: #115894; 
	font-family: 'Microsoft YaHei', 'Verdana', 'SimSun'; 
	float: left;
	font-size: 1.2rem;
}
.volvo-page .path2 .fromList select.input-txt{ 
	width: 72%; 
	box-sizing:border-box;
	padding: 2% 5%; 
	background:  url(../images/ll-img-3.png) 95% center no-repeat;
	background-size: 10px 8px; 
	margin-bottom: 3%;
}
.volvo-page .path2 .fromList select.input-txt option{ 
	color: #000;
}
.volvo-page .path2 .fromList h3.title{ 
	width: 28%;
	float: left;
}
.volvo-page .path2 .fromList .sex-item{
	padding-left: 28%; 
}
.volvo-page .path2 .fromList .sex-item label{
	float: left; 
	width: 25%;
	padding-right: 10%;
}
.volvo-page .path2 .fromList .sex-item label img{
	display: inline-block;
	vertical-align: middle;
	width: 65%;
}
.volvo-page .path2 .fromList .sex-item input{ 
	-webkit-appearance:radio;
	display: inline-block;
}
.volvo-page .path2 .fromList .address-item{
	padding-bottom: 0;
}
.volvo-page .path2 .fromList .address-item #sel{
	float: left;
	width: 72%;
}
.volvo-page .path2 .fromList .address-item #sel .input-txt{
	width: 100%;
}
.volvo-page .path2 .fromList .address-item .title-item{
	float: left;
	width: 28%;
}
.volvo-page .path2 .fromList .address-item .title-item h3.title{
	float: none;
	padding-bottom: 9%;
	width: 100%;
}
.volvo-page .path2 .fromList  .tips-item{
	width: 100%;
	padding-top: 1%;
}
.volvo-page .path2 .fromList .btn-item{ 
	margin-top: 8.125%;
	padding: 0 4.6875%;
}

.volvo-page .path2 .fromList .btn-item:active{ 
	opacity: 0.9
}


.volvo-page .path3{
	background: url(../images/bg-img-2.jpg) center top no-repeat;
	background-size: 100% auto;
	width: 100%;
}

.volvo-page .path4{ 
	padding-top: 17.5%;
	background: url(../images/bg-img-3.jpg) center top no-repeat; 
	background-size: 100% auto; 
	width: 100%; 
	position: relative;
	padding-bottom: 10.5%;
}
.volvo-page .path4 .banner-warp{
	width:90%; 
	margin: 0 4.6875%;
}
.volvo-page .path4 .banner-warp img{
	max-width: 100%;
	display: block;
}
.volvo-page .path4 .banner-warp .swiper-pagination{
	text-align: right;
	width: 98%;
	padding-right: 2%;
}
.volvo-page .path4 .banner-warp .swiper-pagination-bullet{
	height: 5px;
	width: 5px;
	border-radius: 5px;
	margin: 0 2px;
	background:#fff;
	opacity: 1;
}
.volvo-page .path4 .banner-warp .swiper-pagination-bullet-active{
	background: #0b4083;
}
.volvo-page .path4 .banner-warp .txt{  
	width:100%; 
	padding:1.5625% 0 0; 
	color:#171717; 
	height:auto; 
	line-height:1.6; 
	font-size:1.2rem; 
}
.volvo-page .path4 .banner-warp .txt dt{
	text-align: center;
	font-weight: bold;
}
.volvo-page .path4 .banner-warp .txt dd{
	font-size: 1.1rem;
}


.volvo-page .path5{   
	width: 100%;
	background: url(../images/bg-img-4.jpg) center top no-repeat;
	background-size: 100% auto;
	padding-top: 11.875%;
}
.volvo-page .path5 .list{ 
	padding: 0 4.6875%
}
.volvo-page .path5 .list li{ 
	width: 48.5%; 
	float: left; 
	padding-bottom: 1.5625%;
}
.volvo-page .path5 .list li:nth-child(2n+0){
	width: 50%; 
	float: right;
}
.volvo-page .path5 .list li:nth-child(3){ 
	width: 59.48%
}
.volvo-page .path5 .list li:nth-child(4){ 
	width: 38.79%
}

.volvo-page .path-jrzj{
	background: url(../images/ll-img-5.png) center top no-repeat;
	background-size: 100% auto;
	padding: 18.125%  4.6875% 15.625%;
	font-size: 1.3rem;
	text-align: center;
	color: #2f2f2f;
}
.volvo-page .path-jrzj .point{
	color: #052e75;
	font-weight: bold;
	line-height: 1.9;
}
.volvo-page .path-jrzj  .text-item{
	padding-bottom: 6.25%;
}
.volvo-page .path-jrzj .btn-item{
	width: 35.625%;
	margin: 0 auto;
}
.volvo-page .path-jrzj .btn-item:active{
	opacity: 0.9;
}

/* --大图弹出层-- */
.fade{ 
	background: rgba(0,0,0,.9); 
	position: fixed; 
	top: 0; 
	left: 0; 
	width: 100%; 
	height: 100%; 
	z-index: 9
}
.pop-img{ 
	position: fixed; 
	top: 50%; 
	left: 50%; 
	-webkit-transform: translate(-50%,-50%); 
	width: 90.625%;
	z-index: 10
}
.pop-img img{ 
	width: 100%; 
	display: block;
}
.pop-tjing{
	position: fixed; 
	top: 50%; 
	left: 50%; 
	-webkit-transform: translate(-50%,-50%); 
	z-index: 10;
	text-align: center;
	color: #fff;
	font-size: 1.2rem;
}


