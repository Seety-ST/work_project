.picSkill .menu_list li.column-tab.current .list{ padding-bottom: 0px; }
.picSkill .menu_list li.column-tab.current .list li{ width: 100% }
.picSkill .menu_list li.column-tab .list li a,
.picSkill .menu_list li.column-tab.current .list li a{ border-bottom: 0; background:none; float:none; height: 32px; line-height: 32px;}
.picSkill .menu_list li.column-tab.current .list li a:hover,
.picSkill .menu_list li.column-tab.current .list li.cur a{ background-image:none; background-color: #ddd; text-decoration: none; font-weight: bold; }
.picSkill .menu_list li.column-tab a .menu-txt{ float: left; width: 140px; }
.picSkill .menu_list li.column-tab a i.menu-icon{ float: left; width: 30px; background-repeat: no-repeat; }
.picSkill .menu_list li.column-tab a i.new-icon{ background-image: url(../images/icon-new-23x9.png); width: 23px; height: 9px; overflow: hidden; margin-top: 15px; }
.picSkill .menu_list li.column-tab .list li a i.new-icon,
.picSkill .menu_list li.column-tab.current .list li a i.new-icon{ margin-top: 12px;}

