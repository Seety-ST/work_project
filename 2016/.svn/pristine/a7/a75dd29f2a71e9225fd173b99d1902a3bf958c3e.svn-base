/* Photo css */
body { background:#737373 url(http://photo.poco.cn/images_v2/photo_bg.gif) center top repeat-y; font-family:"宋体", Tahoma; line-height:24px; }
/* Link */
a { color:#000; text-decoration:underline; outline:none; }
a:hover { color:#000; text-decoration:none; }
img,
a img { border:0; vertical-align:middle; }
/* Font  */
.fb { font-weight:bold; }
.f12 { font-size:12px; }
.f13 { font-size:13px; }
.f14 { font-size:14px; }
.white { color:#FFF; }
.green { color:#737373; }
.grey { color:#737373; }
.black { color:#000; }
.fn_ta { font-family:Tahoma; }
.fn_st { font-family:宋体; }
/* position */
.left { float: left; }
.right { float: right; }
.clear { clear: both; }
.relative { position:relative; display:inline; }
.text_center { text-align:center!important; }
.text_left { text-align:left!important; }
.text_right { text-align:right!important; }
/*input*/
.inputbg { background:url(http://photo.poco.cn/images_v2/inputbg.jpg) left top no-repeat; padding-left:4px; color:#808080; }
.input180 { width:180px; height:21px; line-height:21px; border:1px solid #737373; }
.input120 { width:120px; height:21px; line-height:21px; border:1px solid #737373;}
.select select { width:116px; height:23px; border:#d2150b; }
.select78 { width:190px; height:23px; border:#737373; }
/* background */
.lh20 { line-height:20px; }
.lh18 { line-height:18px!important; }
.lh30 { line-height:30px!important; }
.mt10 { margin-top:10px!important; }
.mr10 { margin-right:10px!important; }
.mb10 { margin-bottom:10px!important; }
.ml10 { margin-left:10px!important; }
.mb5 { margin-bottom:5px!important; }
.mt5 { margin-top:5px!important; }
.mar0 { margin-top:0px; margin-bottom:0px; }
.mr5 { margin-right:5px!important; }
.mr20 { margin-right:20px!important; }
.mt20 { margin-top:20px!important; }
.mb20 { margin-bottom:20px!important; }
.fn_nm { font-weight:normal!important; }

/*------
背景阴影
-------*/
.shadow { margin:0 auto; background:#737373 url(http://photo.poco.cn/images_v2/doll_bg.gif) center repeat-y; height:100%; }
/*--
栅格背景
------*/
.grid_c2 { background:url(http://photo.poco.cn/images_v2/grid-c2.gif) center top repeat-y }
.grid_c2f { background:url(http://photo.poco.cn/images_v2/grid-c2f.gif) center top repeat-y }
.grid_c2_s312 { background:url(http://photo.poco.cn/images_v2/grid-c2-s312.gif) center top repeat-y; height:100%; }
.grid_c2f_s312 { background:url(http://photo.poco.cn/images_v2/grid-c2f-s312.gif) center top repeat-y; height:100%; }
/*title*/
.c_line { height:1px; line-height:1px; overflow:hidden; background:#737373; margin:10px 0px; clear:both; }
.d_line { background:url(http://photo.poco.cn/images_v2/dot_line.gif) center 8px repeat-x scroll; height:10px; margin:5px 0px; clear:both; }
/*背景线*/
.topic a { color:#737373; }
.topic .more { float:right; text-align:right; font-size:12px; line-height:18px; }
.topic .more a { color:#737373; font-weight:normal; }
h3.title { font-size:14px; font-weight:700; text-indent:22px; background:url(http://photo.poco.cn/images_v2/photo_23.gif) left 2px no-repeat; color:#737373; height:21px; line-height:18px; overflow:hidden; height:18px; margin-bottom:5px; }
h3 a { font-weight:bold; font-size:14px; text-decoration:underline; }
h3 a:hover { font-weight:bold; font-size:14px; text-decoration:none; }
h4 a { font-size:12px; text-decoration:underline; }
h4 a:hover { font-size:12px; text-decoration:none; }
h5 { color:#737373; }
strong a { font-weight:bold; font-size:14px; text-decoration:underline; }
strong a:hover { font-weight:bold; font-size:14px; text-decoration:none; }
.more { text-align:right; font-family: "宋体"; line-height:18px; height:18px; clear:both; }
.more a { font-size:12px; text-decoration:underline; font-family: "宋体"; line-height:18px; height:18px; clear:both; }
.more a:hover { font-size:12px; text-decoration:none; font-family: "宋体" }
#content { margin-bottom:20px; height:100%; }
/*弹出*/
#top_pop { width:992px; background:#f8f8f8; border:#737373 solid 1px; color:#737373; margin:7px auto; height:74px; }
#top_pop .top_colse { width:100px; float:right; color:#000; height:23px; line-height:23px; vertical-align:middle; }
#top_pop .top_colse .checkbox { width:23px; float:left; margin-top:4px; *margin-top:1px;
*margin-top:0px!important;
}
#top_pop .top_colse .text { width:55px; float:left; }
#top_pop .top_colse .close { width:11px; height:11px; float:left; padding-top:5px; }
#top_pop .top_menu { text-align:center; width:992px; height:29px; line-height:29px; }
#top_pop .top_menu li {margin:0px 22px; float:left; text-align:center; display:inline; }
#top_pop .top_menu a { font-size:14px; font-weight:700; color:#737373; text-decoration:underline; }
#top_pop .top_menu a:hover { font-size:14px; font-weight:700; color:#737373; text-decoration:none; }
#top_pop .float_l { float:left; height:35px; line-height:35px; text-indent:18px; width:500px; }
#top_pop .toptitle { width:992px; height:35px; }
/*弹出结束*/
