}
.invite-page .info-item .listItem{
	background: url(../images/line-bg-1x23.png) center repeat-x;
	background-size: contain;
	margin-bottom: 2%;
}
.invite-page .info-item .listItem h3.title{
	font-weight: normal;
	float: left;
	background-color: #fff;
	padding-right: 2%;
}
.invite-page .info-item .listItem .icon{
	float: left;
	height: 0;
	background-size: contain;
	background-repeat: no-repeat;
	background-position: left center;
	background-color: #fff;
}
.invite-page .info-item .listItem .down-icon{
	width: 2.456%;
	padding-top: 6%;
	padding-right: 4%;
	background-image: url(../images/icon-down-14x23.png);
}
.invite-page .info-item ol.list{
	counter-reset: term;
}
.invite-page .info-item ol.list li{
	counter-increment: term;
	counter-reset: definition;
	line-height: 1.5;
	position: relative;
	padding-left: 5%;
}
.invite-page .info-item ol.list li:before{
	content: counter(term) ". ";
	position: absolute;
	top: 0;
	left: 0;
}

/* verify */
.verify-page{
	padding-top: 14%;
}
.verify-page .listItem{
	font-size: 1.6rem;
	color: #4c4c4c;
	text-align: center;
	padding-bottom: 10%;
}
.verify-page .verify-input-wrap .input-item{
	background: #fff;
	margin-bottom: 1.867%;
}
.verify-page .verify-input-wrap .input-item .input-txt{
	float: left;
	width: 75.2%;
	background: url(../images/line-2x88.png) right center no-repeat;
	background-size: contain; 
	box-sizing: border-box;
	padding: 2.67% 5.7%;
	border: 0;
	color: #000;
	font-size: 1.6rem;
	font-family: 'Microsoft YaHei', 'Verdana', 'SimSun';
}
.verify-page .verify-input-wrap .input-item .input-txt::-webkit-input-placeholder{
	color: #d6d6d6
}
.verify-page .verify-input-wrap .input-item .btn-yzm{
	float: left;
	width: 24.8%;
	text-align: center;
	color: #8eb89a;
	font-size: 1.6rem;
	padding: 2.67% 0;
}
.verify-page .verify-input-wrap .input-item .btn-yzm:active{
	opacity: 0.8;
}
.verify-page .verify-input-wrap .input-item .btn-yzm.stop{
	color: #e5e5e5
}
.verify-page .verify-input-wrap .input-item .btn-yzm.stop:active{
	opacity: 1;
}
.verify-page .verify-input-wrap .input-item .btn-yzm.loading{
	font-size: 0;
	position: relative;
	height: 0;
	padding: 11.735% 0 0 0;
}
.verify-page .verify-input-wrap .input-item .btn-yzm.loading::before{
	content: '';
	position: absolute;
	top: 25%;
	left: 38%;
	background: url(../images/loading-icon-44x44.png) no-repeat;
	background-size: contain;
	width: 24%;
	height: 50%;
	-webkit-animation: loadingicon 1s infinite linear;
}
.verify-page .verify-input-wrap .tips-item{
	padding: 0 5.7%;
	color: #666;
	line-height: 1.5;
}
.verify-page .verify-input-wrap .tips-item .icon{
	display: inline-block;
	vertical-align: middle;
	height: 0;
	background-repeat: no-repeat;
	background-size: contain;
}
.verify-page .verify-input-wrap .tips-item .note-icon{
	background-image: url(../images/icon-note-24x24.png);
	width: 3.615%;
	padding-top: 3.615%;
}
.verify-page .verify-input-wrap .tips-item span{
	display: inline-block;
	vertical-align: middle;
}


/* animation */
@-webkit-keyframes loadingicon {
	0% { 
		-webkit-transform: rotate(0deg)  
	}
	100% { 
		-webkit-transform: rotate(360deg) 
	}
}


