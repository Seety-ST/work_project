/* content */
/* task */
.taskhall-page img{
	display: block;
	width: 100%;
}
.taskhall-page .banner-list{
	padding: 5.33% 2.67%;
}
.taskhall-page .banner-list li{
	float: left;
	width: 48.7%;
	height: 0;
	padding-top: 36%;
	margin: 0 1.3% 2.6% 0;
	-webkit-mask-image: url(../images/banner-bg-346x256.png);
	-webkit-mask-size: 100% 100%;
	position: relative;
	background-repeat: no-repeat;
	background-size: contain;
	overflow: hidden;
}
.taskhall-page .banner-list li:nth-of-type(2n){
	margin: 0 0 2.6% 1.3%;
}
.taskhall-page .banner-list .text-item{
	position: absolute;
	left: 0;
	bottom: 0;
	width: 100%;
	height: 30%;
	font-size: 1.2rem;
	line-height: 1.2;
	background:inherit;
	background-size: 100% auto;
	background-position: center bottom;
	background-repeat: no-repeat;
	overflow:hidden;
}
.taskhall-page .banner-list .text-item .text-bg {
	display: block;
	background:inherit;
	background-position: center bottom;
	background-repeat: no-repeat;
	background-size: 100% auto;
	-webkit-filter: blur(5px);
	width: 100%;
	height: 100%;
	z-index: 1;
}
.taskhall-page .banner-list .text-item dl{
	display: block;
	position: absolute;
	left: 0%;
	bottom: 0%;
	width: 100%;
	height: 100%;
	background-color: rgba(255,255,255,.8);
	padding: 3% 4.6%;
	box-sizing: border-box;
	z-index: 2;
}
.taskhall-page .banner-list .text-item dd.info-item .points-item{
	opacity: 0.6;
	float: left;
	margin: 1% 0 0 1%;
}
.taskhall-page .banner-list .text-item dd.info-item .points-item .add,
.taskhall-page .banner-list .text-item dd.info-item .points-item .num{
	float: left;
}
.taskhall-page .banner-list .text-item dd.info-item .icon{
	float: left;
	margin-top: 2%;
	background-repeat: no-repeat;
	background-size: contain;
	height: 0;
}
.taskhall-page .banner-list .text-item dd.info-item .points-icon{
	width: 7%;
	padding-top: 7.33%;
	background-image:url(../images/icon-points-21x22.png);
}

.taskhall-page .footprint-list{
	padding: 3.467% 0 10%;
}
.taskhall-page .footprint-list .listItem{
	padding: 1.5% 3.067%;
	color: #fff;
	opacity: 0.8;
	font-size: 1.2rem;
}
.taskhall-page .footprint-list .item{
	background: rgba(255,255,255,.8);
	color: #535f56;
	font-size: 1.4rem;
	margin-bottom: 2.67%;
}
.taskhall-page .footprint-list .item .date-item{
	font-size: 1.2rem;
	opacity: 0.8;
	border-bottom: 1px solid #dce1e0;
	padding: 1.6% 3.467% 1.4%;
}
.taskhall-page .footprint-list .item dd.operate-item{
	border-bottom: 1px solid #dce1e0;
	margin-left: 7.467%;
	padding: 2.67% 5.3% 2.67% 0;
}
.taskhall-page .footprint-list .item dd:last-child.operate-item{
	border-bottom:0;
}
.taskhall-page .footprint-list .item dd.operate-item .title{
	width: 70%;
	float: left;
}
.taskhall-page .footprint-list .item dd.operate-item .points-item{
	width: 30%;
	text-align: right;
	float: right;
	opacity: 0.8;
}
.taskhall-page .footprint-list .item dd.operate-item .points-item .num{
	margin-left: 2%;
}

