	background-image: url(../images/icon-share-31x38.png);
}
.invite-page .btn-wrap .btn .text-icon{
	padding-top: 5.1%;
	background-image: url(../images/icon-text-31x29.png);
}
.invite-page .btn-wrap .btn .txt{
	display: inline-block;
	vertical-align: middle;
}
.invite-page .info-item{
	font-size: 1.3rem;
	padding-top: 18%;
