/* index */
.index-page{
    min-width: 1280px;
    overflow: hidden;
    position: relative;
}
.index-page .content{
    z-index: 1;
}
.index-page .footer{
    padding: 1.2% 0 0;
}
.index-page .banner-warp{
    animation-name: fadeIn;
    -webkit-animation-name: fadeIn;
    animation-duration: 2s;
    -webkit-animation-duration: 2s;
    animation-delay: 0.5s;
    -webkit-animation-delay: 0.5s;
}
.index-page .banner-warp .flexslider { 
    width: 100%;
    position: relative; 
    height: auto; 
    overflow: hidden; 
}
.index-page .banner-warp .slides { 
    position: relative; 
    z-index: 1;
}
.index-page .banner-warp .slides li { 
    height: auto;
    float: left;
    overflow: hidden;
    background-repeat: no-repeat;
    background-position: center center;  
    background-size: cover;
}
.index-page .banner-warp .slides li .bg-item img{
    width: 100%;
    display: block;
}
.index-page .banner-warp .flex-control-nav { 
    position: absolute; 
    bottom: 20px;
    z-index: 2; 
    width: 100%; 
    text-align: center;
}
.index-page .banner-warp .flex-control-nav li { 
    display: inline-block; 
    width: 12px; 
    height: 12px; 
    margin: 0 8px; 
    *display: inline; 
    zoom: 1;
}
.index-page .banner-warp .flex-control-nav a { 
    display: inline-block; 
    width: 10px; 
    height: 10px; 
    overflow: hidden; 
    border: 1px solid #fff; 
    border-radius: 6px;
    cursor: pointer;
    text-indent: -9999em;
    -moz-transition: all 600ms ease;
    -ms-transition: all 600ms ease;
    -o-transition: all 600ms ease;
    -webkit-transition: all 600ms ease;
    transition: all 600ms ease;
}
.index-page .banner-warp .flex-control-nav .flex-active { 
    background-color: #fff;
}
