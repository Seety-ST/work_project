<script type="text/javascript" src="js/jquery-1.11.0.min.js"></script>
<script type="text/javascript" src="js/iSlider.js"></script> 

<script>
	var myslider=new iSlider({
		wrap:'#wrap',
		item:'.item',
		onslide:function (index) {
		            console.info(index);
		            var audiorestore = $("#audio-restore")[0];
		            var audiobig = $("#audio-big")[0];
		            $(".restore-item").on("click",function(){
		            	audiorestore.play();	//播放
		            	$(".play .mod-animation").show().delay(1000).hide(0);	
		            	$(".play .img-change .cur").delay(600).fadeIn("slow");
		            	$(".play .btn-item").hide();
		            	$(".play .arrow").show();		
		            });
		            $(".largen-item").on("click",function(){
		            	audiobig.play();	//播放
		            	$(".play .mod-animation").show().delay(1000).hide(0);	
		            	$(".play .img-change .cur").delay(600).fadeIn("slow");
		            	$(".play .btn-item").hide();
		            	$(".play .arrow").show();				
		            });
		            $(".share-btn").on("click",function(){
		            	$(".fade").show();
		            	$(".share-pop").show();				
		            });
		},
		loadingImgs:[
			'images/img-item/1/top.jpg',
			'images/img-item/1/mid-normal.jpg',
			'images/img-item/1/mid-change.jpg',
			'images/img-item/1/btm.jpg',
			'images/img-item/2/top.jpg',
			'images/img-item/2/mid-normal.jpg',
			'images/img-item/2/mid-change.jpg',
			'images/img-item/2/btm.jpg',
			'images/img-item/3/top.jpg',
			'images/img-item/3/mid-normal.jpg',
			'images/img-item/3/mid-change.jpg',
			'images/img-item/3/btm.jpg',
			'images/img-item/4/top.jpg',
			'images/img-item/4/mid-normal.jpg',
			'images/img-item/4/mid-change.jpg',
			'images/img-item/4/btm.jpg',
			'images/img-item/5/top.jpg',
			'images/img-item/5/mid-normal.jpg',
			'images/img-item/5/mid-change.jpg',
			'images/img-item/5/btm.jpg',
			'images/img-item/6/top.jpg',
			'images/img-item/6/mid-normal.jpg',
			'images/img-item/6/mid-change.jpg',
			'images/img-item/6/btm.jpg',
			'images/img-item/7/top.jpg',
			'images/img-item/7/mid-normal.jpg',
			'images/img-item/7/mid-change.jpg',
			'images/img-item/7/btm.jpg',
			'images/img-item/8/top.jpg',
			'images/img-item/8/mid-normal.jpg',
			'images/img-item/8/mid-change.jpg',
			'images/img-item/8/btm.jpg'
		],
		onloading:function (complete,total) {
		    this.$('#progress_bg').style.width=complete/total*100+'%';
		    this.$('#progress_icon').style.left=complete/total*100+'%';
		    this.$('#progress_num').innerHTML=parseInt(complete/total*100)+'%';
		    if (complete==total) {
		        this.$('#loading').style.display="none"
		    }
		}
	});
</script>
