body{ color:#808080; background: #fffaf4 url(../images/cj-bg-1.jpg?v=1) center top no-repeat;}




.cj-header{ 
    height: 188px;
    position: repeat
}
.cj-header .cq-btn{
    position: absolute;
    top: 0;
    left: 0;
    background: url(../images/blank.png) repeat;
    width: 169px;
    height: 74px;
}
.cj-header h1.logo{
    position: absolute;
    right: 0;
    top: 0;
    width: 204px;
    height: 55px;
}
.cj-header h1.logo a{
    display: block;
    width: 204px;
    height: 55px;
    background: url(../images/blank.png) repeat;
}
.cj-header .listItem{
    text-align: center;
    color: #52559e;
    font-size: 27px;
    font-weight: bold;
    padding-top: 63px;
    line-height: 40px;
}
.cj-header .listItem dd{
    font-size: 34px;
}

.cj-footer{
    height:91px; 
    background:#f5f5f5 url(../images/cj-footer-1.png) center top no-repeat;
}

/*  mode */
.mod-item{
    width: 494px;
}
.mod-item .listItem{
    background: #52549e;
    width: 474px;
    height: 42px;
    padding: 0 10px;
    color: #fff;
}
.mod-item .listItem h3.title{
    font-size: 20px;
    display: inline-block;
    *+display: inline;
    *+zoom:1;
    _display: inline;
    _zoom:1;
    padding: 5px 10px 0 0;
}
.mod-item .listItem .tips{
    font-size: 11px;
}
.mod-item .listItem .tips .num{
    font-size: 20px;
    color: #ffea00;
    padding: 0 5px;
}
.mod-item .list-warp{
    background: #ebebeb;
    padding: 9px 10px;
    overflow: hidden;
}
.mod-item .list-warp .con-item{
    background: #fff;
    padding: 10px;
}
.mod-item .list-warp .list{
    overflow: auto;
    position: relative;
    height: 508px;
    -webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box;
}
.mod-item .list-warp .list li{
    width: 40%;
    padding: 0 5%;
    height: 22px;
    line-height: 22px;
    float: left;
}

/* scrollbar */
.cj-page .mCSB_scrollTools{
}
.mCSB_scrollTools .mCSB_draggerRail{
    width: 1px;
}
.cj-page .mCS-rounded.mCSB_scrollTools .mCSB_draggerRail{ 
    background-color: #52559e;
}
.cj-page .mCSB_scrollTools .mCSB_dragger:hover .mCSB_dragger_bar,
.cj-page .mCSB_scrollTools .mCSB_dragger .mCSB_dragger_bar{
    background-color: #52559e; 
    background-color: rgba(82,85,158,1);
    filter: "alpha(opacity=100)"; 
    -ms-filter: "alpha(opacity=100)"; 
}



/* content */
.cj-page .path1{
    float: left;
    width: 494px;
    padding-right: 12px;
}
.cj-page .path1 .mod-item .list-warp .list{
    height: 677px;
}
.cj-page .path2{
    float: left;
    width: 494px;
    padding-bottom: 44px;
}
.cj-page .path2 .btn-warp .btn-item{
    display: block;
    width: 100%;
    height: 64px;
    line-height: 64px;
    text-align: center;
    color: #fff;
    font-size: 26px;
    background-color: #6365a6;
    margin-top: 20px;
    font-weight: bold;
}
.cj-page .path2 .btn-warp .btn-item:hover{
    text-decoration: none;
}
.cj-page .path2 .btn-warp .un-btn-item{
    background-color: #8f8f8e
}