body { 
	font: 14px "Microsoft Yahei",Arial,Helvetica,sans-serif; 
	color: #fff;
	background: #fff url(../images/baby/bg-item/bg-img-1920x950.png) center top no-repeat;
	filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='images/baby/bg-item/bg-img-1920x950.png',  sizingMethod='scale');
	background-size: cover;
}


.footer{
	color: #868686;
}
.footer a{
	color: #868686;
}

.page { 
	background-repeat: no-repeat;

}
.contain { 
	position: relative; 
	width: 100%;
	height: 100%;

}
.
.page .con-item{
	width: 1140px;
	margin: 0 auto;
}

#navBar { 
	z-index: 3; 
	position: absolute; 
	top: 40%; 
	right: 24px;
}
#navBar .active { 
	background: url(../images/baby/page-icon-active-12x12.png) no-repeat;
}
#navBar li { 
	cursor: pointer; 
	margin-bottom: 16px; 
	transition: all .7s ease; 
	line-height: 0px; 
	font-size: 0;
	text-align: center; 
	width: 12px; 
	height: 12px;
	background: url(../images/baby/page-icon-12x12.png) no-repeat;
}


.mod-logo{
	width: 9.427%;
	position: fixed;
	top: 0%;
	left: 4.74%;
	z-index: 9;
}
.mod-logo img{
	display: block;
	width: 100%;
}



.mod-download-item{
	width: 474px;
}
.mod-download-item .code-item{
	float: left;
	width: 103px;
}
.mod-download-item .code-item .code-img{
	width: 103px;
	height: 103px;
	overflow: hidden;
	background: #fff;
	border-radius: 0px;
}
.mod-download-item .code-item .code-img img{
	width: 103px;
}
.mod-download-item .btn-warp{
	float: left;
	padding-top: 23px;
	width: 370px;
}
.mod-download-item .btn-warp .btn{
	float: left;
	margin-left: 20px;
	background: url(../images/baby/btn-bg-165x65.png) no-repeat;
	width: 165px;
	height: 65px;
	text-align: center;
	color: #2fb0bb;
	font-size: 0;
	line-height: 65px;
	-webkit-transition: 600ms;
	-moz-transition: 600ms;
	-ms-transition: 600ms;
	-o-transition: 600ms;
	transition: 600ms;
}
.mod-download-item .btn-warp .btn:hover{
	color: #b3dee2;
	text-decoration: none;
	-webkit-transition: 600ms;
	-moz-transition: 600ms;
	-ms-transition: 600ms;
	-o-transition: 600ms;
	transition: 600ms;


}
.mod-download-item .btn-warp .btn .icon{
	display: inline-block;
	*+display: inline;
	*+zoom:1;
	_display: inline;
	_zoom:1;
	vertical-align: middle;
	font-size: 25px;
	margin-right: 10px;
}
.mod-download-item .btn-warp .btn span{
	display: inline-block;
	*+display: inline;
	*+zoom:1;
	_display: inline;
	_zoom:1;
	vertical-align: middle;
	font-size: 22px;
	cursor: pointer;
}

#fp-nav ul li a.active span,
.fp-slidesNav ul li a.active span {
    background: url(../images/baby/page-icon-active-12x12.png) no-repeat;
}
#fp-nav ul li a span,
.fp-slidesNav ul li a span {
    background: url(../images/baby/page-icon-12x12.png) no-repeat;
}

