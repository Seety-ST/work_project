/* checkbox */
.mod-checkbox-item label {
	cursor: pointer;
	position: absolute;
	width: 100%;
	height: 100%;
	top: 0;
  	left: 0;
	background: rgba(50,216,180,0);
	border-radius: 50%;
}
/* Display the tick inside the checkbox */
.mod-checkbox-item label:after {
	opacity: 0;
	content: '';
	position: absolute;
	width: 40%;
	height: 20%;
	background: transparent;
	top: 30%;
	left: 50%;
	margin-left: -20%;
	border: 2px solid #fff;
	border-top: none;
	border-right: none;
	-webkit-transform: rotate(-45deg);
}

/* Create the checkbox state for the tick */
 .mod-checkbox-item input[type=checkbox]:checked + label:after {
 	opacity: 1;
 	background: rgba(50,216,180,1);
 }
.mod-checkbox-item input[type=checkbox]:checked + label{
	background: rgba(50,216,180,1);
}
.mod-checkbox-item input[type=checkbox]:checked {
	-webkit-appearance:none;
	opacity: 0;
	background: transparent;
}


