<ul class="list clearfix">
    {{#each list}}
    <li class="pic-list-width-44-percent">
        <div class="img-item">
            <a href="{{link_url}}"><i class="img paizhao-ph-bgimg" data-lazyload-url="{{images}}"></i></a>
            {{#compare consults ">" 1}}
            <div class="txt f12 color-fff">
                {{consults}}人咨询
            </div>
            {{/compare}}
        </div>
        <div class="txt-item">
            <a href="{{link_url}}">
            <div class="box-1 color-666">
                {{titles}}
            </div>
            </a>
            <div class="box-2 clearfix">
                <div class="txt-1 fldi color-fa6">
                    {{{prices}}}
                </div>
            </div>
        </div>
    </li>
    {{/each}}
</ul>

