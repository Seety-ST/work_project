.fade {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  opacity: .6;
  z-index: 999;
  zoom: 1;
  clear: both;
  _height: 1000px;
  _position: absolute;
  filter: progid:DXImageTransform.Microsoft.gradient(enabled='true', startColorstr='#99000000', endColorstr='#99000000');
  background: rgba(0, 0, 0, 0.6); }

