

.w1200{
  width:1200px;
  margin:0 auto;
}
.content-wrap {
    color: #666;
    .select-nav {
      height: 50px;
      .select-nav-text {
        line-height: 50px;
      }
      .r-arrow {
        margin-left: 15px;
        //margin-right: 15px;
      }
      .main-type {
        position: relative;
        .main-type-list {
          position: absolute;
          top: 35px;
          left: 15px;
          width: 196px;
          display: none;
          height: 40px;
          line-height: 40px;
          border: 1px solid #24c7c7;
          z-index: 1;
          background-color: #fff;
          .main-select-disable {
            color: #ddd;
            padding-left: 10px;
          }
          .main-select-able {
            padding-left: 20px;
            a {
              color: #666;
            }

          }
          .main-select-able a:hover {
            color: #24c7c7;
          }
        }
      }
      .main-type-sel {
        z-index: 2;
        background-color: #fff;
        position: relative;

      }

      .main-type:hover {
        .select-nav-tab {
          border: 1px solid #24c7c7;
          border-bottom: none;
          color: #24c7c7;
          .tab-arrow {
            background: url(../../image/search/main-type-up-12x12.png?__sprite) no-repeat;
          }
        }
        .main-type-sel {
          border-bottom: 0;
        }
        .main-type-list {
          display: block;
        }
      }

      .select-nav-tab {
        cursor: pointer;
        height: 22px;
        border: 1px solid #ddd;
        margin-left: 15px;
        margin-top: 13px;
        .tab-name {
          display: inline-block;
          padding: 0px 0px 0px 10px;
          line-height: 22px;
        }
        .tab-close {
          width: 12px;
          height: 12px;
          display: inline-block;
          background: url(../../image/search/tab-close-12x12.png?__sprite) no-repeat;
          margin: 5px 10px 5px 10px;
        }
        .tab-arrow {
          width: 12px;
          height: 12px;
          display: inline-block;
          background: url(../../image/search/main-type-down-12x12.png?__sprite) no-repeat;
          margin: 5px 10px 5px 10px;
        }
      }
      .select-nav-tab-hover:hover {
        border: 1px solid #24c7c7;
        color: #24c7c7;
        .tab-close {
          background: url(../../image/search/tab-close-hover-12x12.png?__sprite) no-repeat;
        }
      }

    }
    .select-list {
      border: 1px solid #ddd;
      border-bottom: 0;
      .border-line {
        border-bottom: 1px solid #ddd;
      }
      .select-list-tab {
        background-color: #fafafa;
        .tab-title {
          width: 109px;
          padding-top: 16px;
          padding-bottom: 16px;
          padding-right: 21px;
          text-align: right;
          float: left;

        }
        .tab-list {
          padding-left: 20px;
          width: 968px;
          padding-right: 20px;
          float: left;
          background-color: #fff;
          .list {
            li {
              cursor: pointer;
              float: left;
              line-height: 50px;
              margin-left: 20px;
              display: inline-block;
              a {
                color: #666;
              }
            }
            li:hover {
              color: #24c7c7;
            }
            li a:hover {
              color: #24c7c7;
            }
          }
        }
        .tab-list-choose {
          width: 820px;
          padding-left: 20px;
          padding-right: 228px;
          .choose-confirm-btn {
            padding-bottom: 20px;
            cursor: pointer;
            .confirm {
              display: inline-block;
              //background-color: #24c7c7;
              background-color: #ddd;
              color: #fff;
              padding: 5px 10px;
            }
            .confirm-active {
              background-color: #24c7c7;
            }
            .cancel {
              border: 1px solid #d3d3d3;
              background-color: #fff;
              padding: 5px 10px;
            }
          }
          .list-choose {
            li {
              cursor: pointer;
              float: left;
              line-height: 35px;
              margin-left: 20px;
              i {
                width: 10px;
                height: 10px;
                margin-right: 5px;
                position: relative;
                display: inline-block;
                top: 2px;
              }
            }
            .list-li {
              color: #666;
              i {
                background-color: #fff;
                border: 1px solid #666;
              }
            }
            .list-li:hover {
              color: #24c7c7;
              i {
                background-color: #fff;
                border: 1px solid #24c7c7;
              }
            }
            .list-li-cur {
              color: #24c7c7 ;
              i {
                background: url(../../image/search/tab-choose-10x10.png?__sprite) no-repeat;
                border: 1px solid #24c7c7;
              }

            }
          }
        }
        .tab-btn {
          width: 60px;
          float: right;
          background-color: #fff;
          cursor: pointer;
          .btn-text {
            line-height: 50px;
          }
          i {
            width: 12px;
            height: 12px;
            display: inline-block;
            background: url(../../image/search/type-plus-12x12.png?__sprite) no-repeat;
            margin-left: 4px;
            margin-top: 18px;
          }
        }
        .tab-btn:hover {
          color: #24c7c7;
          i {
            background: url(../../image/search/type-plus-hover-12x12.png?__sprite) no-repeat;
          }
        }
      }

    }

    .sort-nav {
      background-color: #FAFAFA;
      .sort-nav-list {
        margin-left: 40px;
        .nav-list-li {
          float: left;
          padding: 15px 0px;
          width: 50px;
          margin-right: 40px;
          cursor: pointer;
          span {
            display: inline-block;

          }
          .icon {
            width: 12px;
            height: 12px;
            position: relative;
            top: 1px;
            display: inline-block;
          }
          .sort-up-arrow {
            display: none;
            background: url(../../image/search/up-arrow-grey-12x12.png?__sprite) no-repeat;
          }
          .sort-down-arrow {
            display: none;
            background: url(../../image/search/down-arrow-grey-12x12.png?__sprite) no-repeat;
          }
        }
        .nav-list-li:hover {
          span {
            color: #24c7c7;
          }
          .sort-up-arrow {
            //isplay: inline-block;
            //background: url(../../image/search/up-arrow-12x12.png?__sprite) no-repeat;
          }
          .sort-down-arrow {
            //display: inline-block;
            //background: url(../../image/search/down-arrow-12x12.png?__sprite) no-repeat;

          }
        }
        .nav-list-li-cur {
          color: #24c7c7;
          .sort-up-arrow {
            display: inline-block;
            background: url(../../image/search/up-arrow-12x12.png?__sprite) no-repeat;
          }
          .sort-down-arrow {
            display: inline-block;
            background: url(../../image/search/down-arrow-12x12.png?__sprite) no-repeat;
          }
        }
        .location-li {
          position: relative;
          .ui-triangle-bottom-mid-gray {
            position: relative;
            top: -1px;
          }
        }
        .nav-list-li-width-auto {
          width: auto;
        }
        .location-li:hover {
          span {
            color: #24c7c7;
          }
          .ui-triangle-bottom-mid-gray {
            position: relative;
            top: -1px;
            display: inline-block;
            border-width: 0px 6px 6px 6px;
            border-color: transparent transparent #24c7c7 transparent;
            _border-color: #000 #000 #fff #000;
            _filter: chroma(color=black);
          }
        }
        .location-li-cur {
          span {
            color: #24c7c7;
          }
          .location-li-list {
            display: block;
          }
          .ui-triangle-bottom-mid-gray {
            position: relative;
            top: -1px;
            display: inline-block;
            border-width: 0px 6px 6px 6px;
            border-color: transparent transparent #24c7c7 transparent;
            _border-color: #000 #000 #fff #000;
            _filter: chroma(color=black);
          }

        }
      }
    }
    .list-wrap {
      .search-result-tips {
        .icon {
          margin: 0 auto
        }
        .icon-no-data-40 {
          background-image: url(../../image/search/no-data-40x40.png);
          background-size: cover;
          background-repeat: no-repeat;
        }
        .ui-icon-size-48 {
          width: 48px;
          height: 48px
        }
        .no-data-tips p {
          margin-top: 15px;
          font-size: 16px
        }
        .no-data-tips {
          .keywords {
            color: #ff5959;
          }
        }
      }
    }
    .search-kw {
      .search-kw-nav {
        background-color: #fffae9;
        border: 1px solid #ffe6c3;
        padding: 9px 0 9px 12px;
        color: #111;
        font-size: 14px;
        .keywords {
          color: #ff5959;
        }
      }
    }
    .page-con {
      text-align: center;
      font-size: 16px;
      padding-top: 10px;
      padding-bottom: 60px;
      .page-mod-1 {
        .click {
          background-color: #24c7c7;
        }
        a {
          border-radius: 3px;
          margin-left: 5px;
          margin-right: 5px;
        }
      }
      .page-go-module {
        line-height: 34px;
        .page-go-input {
          border: 1px solid #ddd;
          width: 30px;
          height: 11px;
          padding: 10px;
        }
        .page-go-btn {
          line-height: 11px;
          border: 1px solid #ddd;
          text-align: center;
          padding: 10px;
          cursor: pointer;
          position: relative;
          top: 2px;

        }
      }
    }





}
/*地址栏样式*/
.location-li-list {
  position: absolute;
  display: none;
  top: 51px;
  left: -280px;
  z-index: 2;
  background-color: #fff;
  border-top:1px solid #ddd;
  width: 440px;
  box-shadow:0 -3px 20px rgba(0, 0, 0, 0.2);
  padding: 20px;
  .location-close-icon {
    position: absolute;
    top: 15px;
    right: 20px;
    width: 16px;
    height: 16px;
    background-image: url(http://paizhao.yueus.com/static/pc/image/ui/close-icon-16x16.png);
    z-index: 3;
}
  .location-select-pop {
    .location-select-pop-item {
      position: relative;
      .item-wrap {
        background-color:#fff;
        .location-tab-list {
          position: absolute;
          z-index: 2;
          li {
            float: left;
            line-height: 21px;
            border: 1px solid #ddd;
            border-bottom: 0;
            text-align: center;
            margin-right: 5px;
            padding-left: 10px;
            padding-right: 10px;
            background-color: #fff;
            .arrow-icon {
              width: 12px;
              height: 12px;
              display: inline-block;
              position: relative;
              top: 1px;
              left: 3px;
              background: url(../../image/search/main-type-down-12x12.png) no-repeat;
            }
          }
          .li-cur {
            line-height: 22px;
            color: #24c7c7;
            border: 1px solid #24c7c7;
            border-bottom: 0;
            .arrow-icon {
              background: url(../../image/search/main-type-down-hover-12x12.png) no-repeat;
            }

          }
          li:hover {
            line-height: 22px;
            border: 1px solid #24c7c7;
            border-bottom: 0;
            color: #24c7c7;
            .arrow-icon {
              background: url(../../image/search/main-type-down-hover-12x12.png) no-repeat;
            }
          }
        }
        .location-tab-detail-list-wrap {
          position: relative;
          top: 22px;
          .location-tab-detail-list {
            border-top: 1px solid #24c7c7;
            overflow: hidden;
            padding-top: 10px;
            padding-bottom: 20px;
            li {
              float: left;
              width: 130px;
              line-height: 25px;
              text-align: left;
              padding-left:10px;
            }
            li:hover {
              color: #24c7c7;
            }
            .li-cur {
              color: #24c7c7;
            }
          }
        }

      }
    }
  }

}


/*地址栏样式结束*/


/*公共样式*/
.color-666 { color: #666 }
.color-ddd { color: #ddd }
.color-fff { color: #fff }
.color-fa { color: #FAFAFA }
.color-d3 { color: #D3D3D3 };

