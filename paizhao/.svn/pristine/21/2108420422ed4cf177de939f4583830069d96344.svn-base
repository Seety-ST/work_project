/*判断当前浏览器是否为ie9以下 start */
.fade{
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    opacity: .6;
    z-index: 9999;
    zoom: 1;
    clear: both;
    _height: 1000px;
    _position: absolute;
    filter: progid:DXImageTransform.Microsoft.gradient(enabled='true', startColorstr='#99000000', endColorstr='#99000000');
    background: rgba(0,0,0,.6);
}
.pop-ie9-fade{
    padding:60px 140px 50px 140px;
    background-color: #fff;
    position:fixed;
    top:50%;
    left:50%;
    margin-left: -300px;
    z-index: 10000;
    margin-top: -110px;
    .close-btn{
        width:14px;
        height:14px;
        position:absolute;
        top:17px;
        right:21px;
        background:url(../../image/footer/close-btn-14x14.png) no-repeat center;
    }
    .item-wrap{
        width:320px;
        .tips-item{
            text-align: center;
            line-height: 26px;
        }
        .btn-item{
            padding-top: 25px;
            .browser-btn{
                margin-left: 20px;
            }
        }
    }
}
 /* 判断当前浏览器是否为ie9以下 end */
