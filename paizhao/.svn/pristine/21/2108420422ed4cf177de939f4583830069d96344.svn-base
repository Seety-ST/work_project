/* 头部开始 */
.global-header{
    background-color: #fff;
    box-shadow: 0px 3px 5px #f0f0f0;
    .lbox{
        .logo-item{
            width:42px;
            height:34px;
            margin-top: 15px;
        }
        .nav-item{
            .list{
                li{
                    height: 64px;
                    line-height: 64px;
                    margin-left: 50px;
                    float: left;
                    .text{
                        padding-bottom: 6px;
                        color: #111111;
                        text-decoration: none;
                    }
                }
                li.cur .text{
                    color: #24c7c7;
                    border-bottom: 3px solid #24c7c7;
                }
                li:hover .text{
                    color: #24c7c7;
                }

            }
        }
    }

    .rbox {
        .seach-area{
            position: relative;
            padding: 20px 20px 0 0;

            .search-icon{
                width: 16px;
                height: 16px;
                display: inline-block;
                background-image: url(../../image/header/search-icon-16x16.png?__sprite);
                background-size: cover;
                display: block;
                cursor: pointer;
                position: absolute;
                right:28px;
                top:25px;
                &:hover{
                    background-image: url(../../image/header/search-icon-16x16-hover.png?__sprite);
                }
            }
            .placeholder-color-a9a{
                color:#a9a9a9;
            }
            .search-icon-first{
                width: 44px;
                height: 16px;
                display: inline-block;
                background-image: url(../../image/header/search-icon-16x16.png);
                background-position: 0px 0;
                background-size: 16px 16px; 
                background-repeat: no-repeat;
                display: block;
                cursor: pointer;
                position: absolute;
                right: 0px;
                top: 25px;
                z-index: 999;
                background-color: #fff;
                &:hover{
                    background-image: url(../../image/header/search-icon-16x16-hover.png);
                }

            }
            .search{
                transition: width .5s;
                // position: relative;
                // width: 300px;
                width:0px;
                overflow: hidden;
                // border-bottom: 1px solid #ddd;
                // padding-bottom: 7px;
                .wbox{
                    border-bottom: 1px solid #ddd;
                    opacity: 0;
                    transition: opacity .5s;
                    position: relative;
                    .lbox{
                        width: 90px;
                        height:28px;
                        .ui-triangle{
                            border-color: #aaaaaa transparent transparent transparent;
                            position: absolute;
                            right:7px;
                            top:9px;
                        }
                        .p1{
                            cursor: pointer;
                            padding-left: 5px;
                        }
                        


                    }
                    .rbox{
                        position: absolute;
                        width:0;
                        overflow: hidden;
                        .input-text{
                            width: 165px;
                            padding: 3px 0 0 10px;
                            width:0;
                            overflow: hidden;
                            transition: width .2s;
                        }
                    }
                    
                }

                .pop-sec{
                    // display:none;
                    position: absolute;
                    top: 28px;
                    left:0px;
                    background-color: #fff;

                    .list{
                        padding: 0px 0 12px 0;
                        width: 90px;
                        border: 1px solid #ddd;
                
                        li{
                            padding: 12px 0 0px 15px;
                            color: #666;
                            font-size: 14px;
                            cursor: pointer;
                            &:hover{
                                color: #24c7c7;
                            }
                        }
                    }
                }

                &:hover{
                    .pop-sec{
                        // display: block;
                    }
                }
            }
            .on{
                width: 300px;
                transition: width .5s;
                overflow: visible;
                .search-icon {
                    z-index: 999
                }
                .wbox{
                    visibility: visible;
                    opacity: 1;
                    width: 300px;
                    .rbox{
                        width:175px;
                        .input-text{
                            width: 165px;
                            padding: 3px 0 0 10px;
                        }
            
                    }
                }
            }
        }
        .info-item{
            margin-top: 16px;
            position: relative;
            z-index: 99
        }
    }

}

/* 公共头部结束 */




