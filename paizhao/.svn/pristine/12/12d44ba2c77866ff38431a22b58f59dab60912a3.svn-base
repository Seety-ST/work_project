<script type="text/javascript">
    
function ajax_auto_action(goods_id,auto_status,auto_type)
{
    if(!confirm('是否确认操作'))
	{
	   return;
	}
    
    if(auto_type == 'sign')
    {
        var url = '?action=goods_auto_action&goods_id='+goods_id+'&is_auto_sign='+auto_status
    }else if(auto_type == 'accept')
    {
         var url = '?action=goods_auto_action&goods_id='+goods_id+'&is_auto_accept='+auto_status
    }else if(auto_type == 'black')
    {
         var url = '?action=goods_add_black&goods_id='+goods_id;
    }else if(auto_type == 'self')
	{
		var url = '?action=goods_auto_action_for_self&goods_id='+goods_id+'&self_is_auto_accept='+auto_status
	}
	
	$.ajax( {    
			url:url,
			type:'post',    
			cache:false,    
			dataType:'json',    
			success:function(data) {    
			    alert(data.message);
			    /*   
				if(data.result==1){    
					alert('修改成功');
					location.reload();
				}else{    
					alert(data.message);  
				}
				*/    
			 },    
			 error : function() {    
				  alert("异常！");
			 }    
	});  
}
    
function change_status(id,status)
{
	if(!confirm('是否确认修改状态？'))
	{
	   return;
	}
	$.ajax( {    
			url:'?action=goods_chstatus&id='+id+'&status='+status,
			type:'post',    
			cache:false,    
			dataType:'json',    
			success:function(data) {    
			    alert(data.message);
			    /*   
				if(data.result==1){    
					alert('修改成功');
					location.reload();
				}else{    
					alert(data.message);  
				}
				*/    
			 },    
			 error : function() {    
				  alert("异常！");
			 }    
	});  
}
function change_show(id,status)
{
	var $con_var = '是否确定上架?';
	if(status==2)
	{
		$con_var = '是否确定下架?';
	}
	if(!confirm($con_var))
	{
	   return;
	}
	$.ajax( {    
			url:'?action=goods_chshow&id='+id+'&status='+status,
			type:'post',    
			cache:false,    
			dataType:'json',    
			success:function(data) {    
