.ui-button {
  cursor: pointer; }
  .ui-button .ui-button-content {
    padding: 0px 20px; }

.ui-button-100per {
  width: 100%; }

.ui-button-block {
  display: block; }

.ui-button-border-24c {
  background-color: #fff;
  color: #24c7c7;
  border: 1px solid #24c7c7; }

.ui-button-bg-24c {
  background-color: #24c7c7;
  color: #fff; }

.ui-button-size-h45 {
  height: 45px;
  line-height: 45px; }

.ui-button-size-h50 {
  height: 50px;
  line-height: 50px; }
