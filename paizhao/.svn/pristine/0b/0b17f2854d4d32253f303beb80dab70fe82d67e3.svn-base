/*页面样式*/
/*滚动*/
.index-page .p1th .scrollBox {
	width: 100%;
	position: relative;
	overflow: hidden;
	height: 512px;
}
.index-page .p1th .scrollBox 	.img_list{
	width: 100%;
}
.index-page .p1th .scrollBox 	.img_list li{
	width: 100%;
	height: 512px;
	float: left;
	display: block;
	background-size: cover;
	background-repeat: no-repeat;
	background-position: center top;	
	
}



.index-page .p1th .scrollBox .page-wrap .page-box{
	position: absolute;
	left: 50%;
	bottom: 20px;
	margin-left: -96px;
	width: 200px;
	text-align: center;
}
.index-page .p1th .scrollBox .page-wrap .page-box a {
	width: 10px;
	height: 10px;
	display: inline-block;
	border-radius: 10px;
	background: #fff;
	margin-right: 10px;
	text-indent: -9999px;
	opacity: 0.5;
	-webkit-box-shadow: 0 0 4px #bbb;
	-moz-box-shadow: 0 0 4px #bbb;
	box-shadow: 0 0 4px #bbb;

}
.index-page .p1th .scrollBox .page-wrap .page-box a:hover, 
.index-page .p1th .scrollBox .page-wrap .page-box a.selected {
	background: #fff;
	opacity: 1;
}
.index-page .p1th .scrollBox .page-wrap .pre{
	position: absolute;
	top:50%;
	margin-top: -22px;
	width: 44px;
	height:44px;
	left: -60px;
	background: url() no-repeat;
	-webkit-transition: right .2s, left .2s;
	-moz-transition: right .2s, left .2s;
	-ms-transition: right .2s, left .2s;
	-o-transition: right .2s, left .2s;
	transition: right .2s, left .2s;
}
.index-page .p1th .scrollBox .page-wrap .next{
	top:50%;
	margin-top: -22px;
	width: 44px;
	height:44px;
	background: url() no-repeat;
	position: absolute;
	right: -60px;
	display:none;
	-webkit-transition: right .1s, left .1s;
	-moz-transition: right .1s, left .1s;
	-ms-transition: right .1s, left .1s;
	-o-transition: right .1s, left .1s;
	transition: right .1s, left .1s;
}


