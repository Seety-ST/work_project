<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=GBK">
<script type="text/javascript" src="./js/jquery.min.js"></script>
<script language="javascript" type="text/javascript" src="js/DatePicker/WdatePicker.js"></script>
<link rel="stylesheet" type="text/css" href="css/style.css">
<link type="text/css" href="css/jquery-ui-1.8.17.custom.css" rel="stylesheet" />
<link type="text/css" href="css/jquery-ui-timepicker-addon.css" rel="stylesheet" />
<script type="text/javascript" src="js/jquery-ui-1.8.17.custom.min.js"></script>

<script type="text/javascript" src="js/jquery-ui-timepicker-addon.js"></script>
<script type="text/javascript" src="js/jquery-ui-timepicker-zh-CN.js"></script>

<script src="js/shadowbox/shadowbox.js" type="text/javascript"></script>
<link href="js/shadowbox/shadowbox.css" rel="stylesheet" type="text/css" />
<style>
    table th {text-align: left;}
</style>
<script type="text/javascript"> 
$(function () {
	$(".ui_timepicker").datetimepicker({
		//showOn: "button",
		//buttonImage: "./css/images/icon_calendar.gif",
		//buttonImageOnly: true,
		showSecond: true,
		timeFormat: '',
		stepHour: 1,
		stepMinute: 1,
		stepSecond: 1
	});
});
Shadowbox.init({ 
    handleOversize: "drag", 
	overlayColor: '#000',
    modal: true,
    displayNav: true,
//	onClose: function(){location.reload();},
	displayCounter: false
}); 
Shadowbox.setup();

function export_ex(obj)
{
	obj.form.target='_blank';
	$('#action').attr('value','consult_button_export');
	obj.form.submit();	
	obj.form.target='_self';
	$('#action').attr('value','consult_button');
}

function change_select()
{
	if ($('#consult_type').val() == 'seller')
	{
		$('#goods_type_id').attr("disabled","disabled");
		$('#goods_style_id').attr("disabled","disabled");
	}
	else
	{
		$('#goods_type_id').removeAttr("disabled");
		$('#goods_style_id').removeAttr("disabled");
	}
}
</script>
<title>咨询按钮转化报表</title>
</head>
<body>
<div class="mainbox">
<table class="search_table" width="100%">
	<tbody>
		
        <tr>
			<td width="81%" class="search">
			  <form action="" method="post" id='search_form'>
			  搜索类型(选择商家则风格和类型无法作为条件):
			 <select name="consult_type" id="consult_type" onchange="change_select();">                      
                <option value="seller" <!-- IF consult_type="seller" -->selected="selected"<!-- ENDIF --> >商家</option>
             	<option value="goods" <!-- IF consult_type="goods" -->selected="selected"<!-- ENDIF --> >商品</option>
             </select>
             &nbsp;&nbsp;&nbsp;
			   日期: 
			    <input placeholder='开始' id="start_time" type="text" size="10" name="start_time" value="{start_time}" class="ui_timepicker" readonly="readonly">
			    ――
			    <input placeholder='结束' id="end_time" type="text" size="10" name="end_time" value="{end_time}" class="ui_timepicker" readonly="readonly">
			 &nbsp;&nbsp;&nbsp;
			  商品类型:
                <select name="goods_type_id" id="goods_type_id">                      
                    <option value="">全部</option>
                    <!-- BEGIN goods_type_arr -->
                    	<option value="{id}" <!-- IF selected="1" -->selected="selected"<!-- ENDIF --> >{name}</option>
                    <!-- END status_arr -->
                </select>
             &nbsp;&nbsp;&nbsp;
			   场景风格:
                <select name="goods_style_id" id="goods_style_id">                      
                    <option value="">全部</option>
                    <!-- BEGIN goods_style_arr -->
                    	<option value="{id}" <!-- IF selected="1" -->selected="selected"<!-- ENDIF --> >{name}</option>
                    <!-- END status_arr -->
                </select> 
                <br />
           	  关键字: 
               <input style='width:208px' type='text' name='search_word' value="{search_word}" placeholder="摄影师名字/摄影师ID/商品名称/商品ID">
               &nbsp;&nbsp;
               <input type="hidden" id="action" name="action" value="consult_button"/>
               <input type="hidden" name="source" id='source_hide' value="{source}">
               <input type="submit" class="btn_class" name="button" id="button" value="查询" />&nbsp;&nbsp;&nbsp;&nbsp;
               <input type="button" name="button2" id="button2" value="导出CSV" onclick="if(confirm('确定导出?')){export_ex(this);}"/>
              </form>
			</td>
			
		</tr>
	</tbody>
</table>
 
    <div id="tabs">
    <div class="tabbox">
      <div class="table-list">
        <table width="100%" cellspacing="0">
                    <thead>
                      <tr>
                        <th>日期</th>  
                        <th>商品名称</th>
                        <th>价格</th>
                        <th>商品类型</th>
                        <th>场景风格</th>
                        <th>商家名字</th>
                        <th>咨询次数</th>  
                        <th>提交次数</th>
                        <th>转化</th>
                        <th>
                        	<select id='source' onchange="submit_form();">
                        		<option value="" <!-- IF source="" -->selected="selected"<!-- ENDIF --> >来源</option>
                        		<option value='PC' <!-- IF source="PC" -->selected="selected"<!-- ENDIF --> >PC</option>
                        		<option value='WAP' <!-- IF source="WAP" -->selected="selected"<!-- ENDIF --> >WAP</option>
                        	</select>
                        </th>
                      </tr>
                    </thead>
                    <tbody>
                      <!-- BEGIN list -->
                      <tr>
                          <td>{create_time}</td>
                          <td>{titles}</td>
                          <td>{prices}</td>
                          <td>{type_name}</td>
                          <td>{style_name}</td>
                          <td>{seller_name}</td>
                          <td>{consult_click_num}</td>
                          <td>{consult_num}</td>
                          <td>{ratio}</td>
                          <td>{source}</td>
                      </tr>
                      <!-- END list -->
                    </tbody>
        </table>
      </div>
    </div>
    <div id="pages" class="page"> {page} </div>  
</div>
</body>
</html>
<script>
function submit_form()
{
	var source = $('#source').val();
	$('#source_hide').val(source);
	$('#search_form').submit();
}
</script>