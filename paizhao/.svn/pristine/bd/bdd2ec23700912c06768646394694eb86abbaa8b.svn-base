.goods-detail-page {
  padding-bottom: 70px; }
  .goods-detail-page .p1th {
    padding-bottom: 10px;
    background-color: #f5f5f5; }
    .goods-detail-page .p1th .item-wrap {
      background-color: #fff; }
      .goods-detail-page .p1th .item-wrap .img-item .img {
        width: 100%;
        padding-top: 100%;
        display: inline-block;
        background-size: cover;
        background-repeat: no-repeat; }
      .goods-detail-page .p1th .item-wrap .txt-item {
        padding: 8px 15px 10px 15px; }
        .goods-detail-page .p1th .item-wrap .txt-item .box-1 {
          color: #111111;
          line-height: 25px; }
          .goods-detail-page .p1th .item-wrap .txt-item .box-1 .type-txt {
            padding: 2px 5px;
            background-color: #fbb939;
            color: #fff;
            margin-right: 8px; }
        .goods-detail-page .p1th .item-wrap .txt-item .box-2 {
          padding-top: 3px; }
          .goods-detail-page .p1th .item-wrap .txt-item .box-2 .lbox {
            font-size: 21px; }
          .goods-detail-page .p1th .item-wrap .txt-item .box-2 .rbox {
            margin-top: 6px; }
      .goods-detail-page .p1th .item-wrap .promise-item {
        padding: 9px 15px;
        background-color: #fcfcfc; }
        .goods-detail-page .p1th .item-wrap .promise-item .list li {
          float: left;
          margin-right: 15px;
          background-color: #fcfcfc; }
          .goods-detail-page .p1th .item-wrap .promise-item .list li .icon-right {
            background: url(data:image/png;base64,) no-repeat center;
            background-size: 100%;
            vertical-align: middle; }
          .goods-detail-page .p1th .item-wrap .promise-item .list li .txt {
            display: inline-block;
            vertical-align: middle;
            margin-left: 5px; }
  .goods-detail-page .p2th {
    padding: 20px 15px;
    background-color: #fff; }
    .goods-detail-page .p2th .item-wrap .title-item {
      margin-bottom: 3px; }
      .goods-detail-page .p2th .item-wrap .title-item .i-iconfont-allow {
        color: #44454a; }
      .goods-detail-page .p2th .item-wrap .title-item .i-iconfont-allow-rotate {
        transform: rotate(180deg);
        -webkit-transform: rotate(180deg); }
      .goods-detail-page .p2th .item-wrap .title-item .select-txt {
        margin-top: 1px; }
    .goods-detail-page .p2th .item-wrap .item .list li {
      padding: 4px 9px;
      border: 1px solid #ddd;
      float: left;
      margin: 10px 10px 0px 0px; }
      .goods-detail-page .p2th .item-wrap .item .list li.cur {
        border-color: #24c7c7;
        color: #fff;
        background-color: #24c7c7; }
    .goods-detail-page .p2th .item-wrap .item .list.tips-bg {
      animation: 3s order-blink-smooth 3;
      -webkit-animation: 3s order-blink-smooth 3; }
  .goods-detail-page .p3th .item-wrap .seller-info-item {
    background-color: #fff;
    padding: 15px;
    margin: 10px;
    border-radius: 4px;
    -webkit-border-radius: 4px;
    border: 1px solid #ddd; }
  .goods-detail-page .p3th .item-wrap .txt-item {
    margin-left: 10px; }
    .goods-detail-page .p3th .item-wrap .txt-item .box-1 {
      margin-top: 3px; }
  .goods-detail-page .p3th .item-wrap .i-iconfont-allow {
    margin-top: 13px;
    color: #44454a;
    transform: rotate(270deg);
    -webkit-transform: rotate(270deg); }
  .goods-detail-page .p4th .item-wrap .nav-item {
    background-color: #fff; }
    .goods-detail-page .p4th .item-wrap .nav-item .list {
      display: -webkit-box; }
      .goods-detail-page .p4th .item-wrap .nav-item .list li {
        width: 50%;
        padding: 12px 0px;
        text-align: center;
        background-color: #fafafa;
        border-bottom: 1px solid #ddd; }
        .goods-detail-page .p4th .item-wrap .nav-item .list li.cur {
          border-bottom: none;
          background-color: #fff;
          background-image: url(data:image/jpeg;base64,);
          background-size: 100% 2px;
          background-repeat: repeat-x;
          background-position: left top; }
        .goods-detail-page .p4th .item-wrap .nav-item .list li.border-right {
          border-right: 1px solid #ddd; }
  .goods-detail-page .p4th .item-wrap .content .bottom-content {
    background-color: #fff;
    margin-bottom: 10px;
    padding: 15px;
    font-size: 14px; }
    .goods-detail-page .p4th .item-wrap .content .bottom-content img {
      width: 100%; }
  .goods-detail-page .p4th .item-wrap .content .detail-item .top-content {
    margin-bottom: 10px;
    background-color: #fff; }
    .goods-detail-page .p4th .item-wrap .content .detail-item .top-content .list li {
      padding: 15px;
      display: -webkit-box; }
      .goods-detail-page .p4th .item-wrap .content .detail-item .top-content .list li:last-child {
        border-bottom: none; }
      .goods-detail-page .p4th .item-wrap .content .detail-item .top-content .list li .lbox {
        width: 20%; }
      .goods-detail-page .p4th .item-wrap .content .detail-item .top-content .list li .rbox {
        -webkit-box-flex: 1; }
  .goods-detail-page .p4th .item-wrap .content .size-item .top-content {
    margin-bottom: 10px;
    background-color: #fff; }
    .goods-detail-page .p4th .item-wrap .content .size-item .top-content .list li {
      padding: 15px;
      border-bottom: 1px solid #ddd;
      display: -webkit-box; }
      .goods-detail-page .p4th .item-wrap .content .size-item .top-content .list li:last-child {
        border-bottom: none; }
      .goods-detail-page .p4th .item-wrap .content .size-item .top-content .list li .lbox {
        width: 20%; }
      .goods-detail-page .p4th .item-wrap .content .size-item .top-content .list li .rbox {
        -webkit-box-flex: 1; }
  .goods-detail-page .p5th {
    background-color: #fff; }
    .goods-detail-page .p5th .item-wrap .title-item {
      padding-top: 4%; }
    .goods-detail-page .p5th .item-wrap .more-item {
      padding: 12px 0px; }
      .goods-detail-page .p5th .item-wrap .more-item .more-txt {
        display: inline-block;
        vertical-align: middle; }
      .goods-detail-page .p5th .item-wrap .more-item .jiantou-icon {
        width: 16px;
        height: 16px;
        display: inline-block;
        vertical-align: middle;
        background: url(data:image/png;base64,) no-repeat center;
        background-size: 100%; }
  .goods-detail-page .consult-item {
    width: 100%;
    position: fixed;
    left: 0;
    bottom: 0; }

.layui-m-layer-pai_zhao_goods_class {
  position: relative; }
  .layui-m-layer-pai_zhao_goods_class .pop-contact-mod .it {
    margin: 10px 0px; }
    .layui-m-layer-pai_zhao_goods_class .pop-contact-mod .it .input-text {
      width: 92%;
      padding: 12px 4%;
      border: 1px solid #ddd;
      margin-top: 15px; }
  .layui-m-layer-pai_zhao_goods_class .pop-contact-mod .ui-button {
    margin-top: 15px; }
  .layui-m-layer-pai_zhao_goods_class .pop-contact-mod .num-box {
    margin-top: 15px; }
  .layui-m-layer-pai_zhao_goods_class .pop-contact-mod .close-btn {
    width: 22px;
    height: 22px;
    display: inline-block;
    position: absolute;
    top: 11px;
    right: 15px;
    background: url(data:image/jpeg;base64,) no-repeat center;
    background-size: contain; }
  .layui-m-layer-pai_zhao_goods_class .pop-contact-mod .ui-button-bg-24c:active {
    background-color: #22bdbd; }

body .layui-m-layer .layui-m-layer-msg {
  bottom: 50%; }

@keyframes order-blink-smooth {
  0 {
    background-color: rgba(255, 180, 182, 0.5); }

  50% {
    background-color: rgba(255, 180, 182, 0.3); }

  100% {
    background-color: rgba(255, 180, 182, 0.2); } }
