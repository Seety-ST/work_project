.ui-avatar-icon i {
  width: 100%;
  height: 100%;
  display: block;
  background-size: cover;
  border-radius: 50%;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

.ui-avatar-icon-32 {
  width: 0.32rem;
  height: 0.32rem;
}

.ui-avatar-icon-45 {
  width: 0.45rem;
  height: 0.45rem;
}

.ui-avatar-icon-60 {
  width: 0.6rem;
  height: 0.6rem;
}