.index-page{
	.item-wrap{
		.search-item{
			padding:7px 15px;
			display: -webkit-box;
			.lbox{
				position:relative;
				width:81px;
				.item{
					padding:5px 25px 0px 0px;
					position:relative;
					.arrow-icon{
						width:10px;
						height: 10px;
						position: absolute;
						right:10px;
						top:10px;
						display:inline-block;
						vertical-align: middle;
						background-image: url(../../image/index/arrow-icon-20x20.png?__sprite);
						background-size: 10px 10px;
						&.arrow-icon-hover{
							background-image: url(../../image/index/arrow-icon-hover-20x20.png?__sprite);
							background-size: 10px 10px;

						}
					}
					.txt{
						

					}
				}
			
				.pop-item{
					background-color: #fff;
					position:absolute;
					left: 0;
					
					z-index: 99;
					box-shadow: 0 0 20px rgba(0, 0, 0, 0.5);
					&.top42{
						top:42px;
					}
					&.top011{
						top:-120px;
					}
					.triangle-icon{
							width: 0;
							height: 0;
							border-left: 10px solid transparent;
							border-right: 10px solid transparent;
							border-bottom: 10px solid #fff;
							position: absolute;
							top: -10px;
							left: 50%;
							margin-left: -10px;
					}
					.list{
						padding:0px 10px;
						li{
							padding:13px 0px;
							border-bottom: 1px solid #ddd;
							&:last-child{
								border-bottom:none;
							}
						}
					}
				}
				
			}
			.rbox{
				-webkit-box-flex:1;
				.input-txt{
					padding:0px 4%;
					width:92%;
					height:30px;
					background-color: #eeeeee;
				}
			}
		}
		.content{
			.p1th{
				.swiper-container{
					position:relative;
					.swiper-wrapper{
						.swiper-slide{
							.img{
								width:100%;
								min-height: 160px;
								display: block;
							}
						}
					}
					
					.swiper-pagination{
						width:100%;
						position:absolute;
						left:0;
						bottom:2.667%;
						text-align: center;
						.swiper-pagination-bullet{
							width:8px;
							height:8px;
							border-radius: 50%;
							display:inline-block;
							margin-right: 3px;
							background: rgba(0,0,0,.5);
							border:1px solid #a8a6aa;
							&.swiper-pagination-bullet-active{
								border-color:#fff;
							}
						}
					}
				}
				
			}
			.p2th{
				background-color: #fff;
				.list{
					li{
						width:23.5%;
						
						float: left;
						position:relative;
						margin:1.45% 0px 0px 1.05%;
						.img{
							width:100%;
							height:100%;
							img{
								width:100%;
								min-height: 88px;
								display:block;
							}
						}
						.txt-item{
							width:100%;
							height:100%;
							position:absolute;
							left:0;
							top:0;
							text-align: center;
							color:#fff;
							background: rgba(0,0,0,.4);
							
							.txt{
								width:100%;
								height:100%;
								display:table;
								span{
									display:table-cell;
									vertical-align: middle;
								}
							}
						}
						&.all-list-style{
							width:23%;
							border:2px solid #24c7c7;
							.img{
								img{
									min-height: 84px;
								}
							}
							.txt-item{
								color:#24c7c7;
								
								background: rgba(255,255,255,.9);
								.txt{
									.arrow-icon{
										width:12px;
										height:12px;
										display:inline-block;
										background:url(../../image/index/arrow-icon-24x24.png) no-repeat   center;
										background-size: 12px 12px;
									}
								}
								
							}
						}
					}
				}
			}
			.p3th{
				padding:10px 0px;
				background-color: #fff;
				.list{
					li{
						width:33.33%;
						float:left;
						&.even{
							width:50%
						}
						&.odd{
							width:33.33%;
						}
						.img-item{
							width:35.2%;
							padding-top: 35.2%;
							position:relative;
							margin:10px auto;
							.img{
								width:100%;
								height:100%;
								position:absolute;
								left:0;
								top:0;
								background-size: cover;
							}
						}
						.txt-item{
							text-align: center;
							margin-bottom: 10px;
							overflow: hidden;
							text-overflow: ellipsis;
							display: -webkit-box;
							-webkit-line-clamp: 1;
							-webkit-box-orient: vertical;
						}
					}
				}
			}
			
			.p4th{
				padding:15px 0px;
				margin-top: 10px;
				background-color: #fff;
				.title-item{
					padding:0px 15px;
					.lbox{
						.en-txt{
							padding-bottom: 7px;
							background: url(../../image/index/line-img-40x4.png) no-repeat left bottom;
							background-size: 20px 2px;

						}
						.cn-txt{
							padding-top: 7px;
						}
						
					}
					.rbox{
						padding-top: 30px;
						.jiantou-icon{
							width:16px;
							height:16px;
							display:inline-block;
							vertical-align: middle;
							background: url(../../image/index/jiantou-icon-32x32.png) no-repeat center;
							background-size: 100%;
						}
						.more{
							display:inline-block;
							vertical-align: middle;
						}
					}
				}
				.item-wrap{
					padding-top: 25px;
					.swiper-container{
						.swiper-wrapper{
							.swiper-slide{
								width:50%;
								margin-bottom: 20px;
								margin-right:8%;
								box-shadow: 0 0 20px rgba(0,0,0,.2);
								opacity: .3;
								.img-item{
									width:100%;
									.img{
										width:100%;
										img{
											width:100%;
											display:block;
										}
									}
								}
								.txt-item{
									padding:10px;
									display:-webkit-box;
									.lbox{
										width:45px;
										height:45px;
										.avatar-icon{
											width:100%;
											img{
												width:100%;
												display:block;
											}
										}
									}
									.rbox{
										-webkit-box-flex:1;
										padding:0px 3%;
										text-align: center;
										.box-1{
											overflow: hidden;
											text-overflow: ellipsis;
											display: -webkit-box;
											-webkit-line-clamp: 1;
											-webkit-box-orient: vertical;
										}
										.box-2{
											padding-top: 5px;
											.tips-txt{
												padding:2px;
												font-size: 10px;
												background-color: #24c9c9;
												color:#fff;
											}
										}
									}
								}
								&.swiper-slide-active{
									opacity:1;
								}
							}

						}
					}

				}
			}
			.p5th{
				padding:15px 0px 0px 0px;
				margin-top: 10px;
				background-color: #fff;
				.title-item{
					padding:0px 15px;
					.lbox{
						.en-txt{
							padding-bottom: 7px;
							background: url(../../image/index/line-img-40x4.png) no-repeat left bottom;
							background-size: 20px 2px;

						}
						.cn-txt{
							padding-top: 7px;
						}
						
					}
					.rbox{
						padding-top: 30px;
						.jiantou-icon{
							width:16px;
							height:16px;
							display:inline-block;
							vertical-align: middle;
							background: url(../../image/index/jiantou-icon-32x32.png) no-repeat center;
							background-size: 100%;
						}
						.more{
							display:inline-block;
							vertical-align: middle;
						}
					}
				}
				.item-wrap{
					padding-top: 25px;
					padding:25px 0px 20px 15px;
					overflow-x:auto;
					.list{
						display:-webkit-box;
						a{
							width:100%;
						}
						li{
							width:37.3%;
							margin-right: 2.6%;
							.con-item{
								width:100%;
								.img-item{
									width:100%;
									padding-top: 100%;
									position:relative;
									.img{
										width:100%;
										height:100%;
										display:inline-block;
										position:absolute;
										left:0;
										top:0;
										background-size: cover;
									}
								}
								.txt-item{
									position:relative;
									.box-1{
										padding-top: 24px;
										.ui-avatar-icon-45{
											width:45px;
											height:45px;
										}
										.ui-avatar-icon{
											position:absolute;
											top: -23px;
											left: 50%;
											margin-left: -23px;
											i{
												width:100%;
												height:100%;
												display: block;
												background-size: cover;
												border-radius: 50%;
												border:2px solid #fff;
											}
										}
									}
									.box-2{
										.txt-1{
											padding:12px 0px 6px 0px;
											background: url(../../image/index/line-img-40x2.jpg) no-repeat center bottom;
											background-size: 20px 1px 
										}
									}
									.box-3{
										.txt-1{
											padding-top: 4px;
										}
									}
								}
							}
						}
					}
				}
			}
			.p6th{
				padding-top:15px;
				margin-top: 10px;
				background-color: #fff;
				.title-item{
					padding:0px 15px;
					.lbox{
						.en-txt{
							padding-bottom: 7px;
							background: url(../../image/index/line-img-40x4.png) no-repeat left bottom;
							background-size: 20px 2px;

						}
						.cn-txt{
							padding-top: 7px;
						}
						
					}
					
				}
				.more-item{
					padding:15px 0px;
					text-align: center;
					background-color: #fff;
					border-top:1px solid #ddd;
					.jiantou-icon{
						width:16px;
						height:16px;
						display:inline-block;
						vertical-align: middle;
						background: url(../../image/index/jiantou-icon-32x32.png) no-repeat center;
						background-size: 100%;
					}
					.more{
						display:inline-block;
						vertical-align: middle;
					}
				}
				.item-wrap{
					padding:20px 0px 0px 15px;
					.list{
						overflow: hidden;
						li{
							padding-bottom: 20px;
							margin-right: 4%;
							float: left;
							.img-item{
								width:100%;
								padding-top: 100%;
								position:relative;
								.img{
									width:100%;
									height:100%;
									display:inline-block;
									position:absolute;
									left:0;
									top:0;
									background-size: cover;
								}
								.txt{
									position:absolute;
									right:0;
									bottom:0;  
									background:rgba(0,0,0,.5);
									padding:1.4%;
								}
							}
							.txt-item{
								.box-1{
									.txt-1{
										padding-top: 10px;
										height:42px;
										overflow: hidden;
										text-overflow: ellipsis;
										display: -webkit-box;
										-webkit-line-clamp: 2;
										-webkit-box-orient: vertical;
									}
								}
								.box-2{
									.txt-1{
										padding-top: 5px;
									}
								}
							}
							&.max-size{
								width:96%;
								.txt-item{
									.box-1{
										text-align: center;
										height:31px;
										overflow:hidden;
										text-overflow:ellipsis;
										white-space: nowrap;
									}
									.box-2{
										text-align: center;
									}
								}
							}
							&.min-size{
								width:46%;
							}
						}
					}

				}
			}
		}
	}
}