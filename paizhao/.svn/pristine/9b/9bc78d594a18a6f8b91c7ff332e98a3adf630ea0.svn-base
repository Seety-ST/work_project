

.w1200{
  width:1200px;
  margin:0 auto;
}
.content-wrap {
    .select-nav {
      height: 50px;
      .select-nav-text {
        line-height: 50px;
      }
      .r-arrow {
        margin-left: 15px;
        margin-right: 15px;
      }
      .main-type {
        position: relative;
        z-index: 5;
        .main-type-list {
          display: none;
          position: absolute;
          height: 40px;
          line-height: 40px;
          top: 25px;
          border: 1px solid #49DED7;
          z-index: 4;
          .main-select-disable {
            color: #ddd;
          }
          .main-select-able {
            color: #666;
          }
          .main-select-able:hover {
            color: #49DED7;
          }
        }
      }
      .main-type:hover {
        .main-type-list {
          display: block;
        }
      }

      .select-nav-tab {
        height: 22px;
        border: 1px solid #ddd;
        margin-left: 10px;
        margin-top: 14px;
        .tab-name {
          display: inline-block;
          padding: 5px 10px;
        }
        .tab-close {
          width: 12px;
          height: 12px;
          display: inline-block;
          background: url(../../image/search/tab-close-12x12.png?__sprite) no-repeat;
          margin: 5px 10px 5px 10px;
        }
        .tab-arrow {
          width: 12px;
          height: 12px;
          display: inline-block;
          background: url(../../image/search/main-type-down-12x12.png?__sprite) no-repeat;
          margin: 5px 10px 5px 10px;
        }
      }
      .select-nav-tab-hover {
        border: 1px solid #49DED7;
        border-bottom: none;
        .tab-arrow {
          background: url(../../image/search/main-type-up-12x12.png?__sprite) no-repeat;
        }
      }
    }

    .select-list {
      border: 1px solid #ddd;
      .border-line {
        border-bottom: 1px solid #ddd;
      }
      .select-list-tab {
        height: 50px;
        .tab-title {
          width: 109px;
          background-color: #FAFAFA;
          line-height: 50px;
          padding-right: 21px;

        }
        .tab-list {
          margin-left: 20px;
          .list {
            li {
              float: left;
              line-height: 50px;
              margin-left: 20px;
            }
          }
        }
        .tab-btn {
          width: 60px;
          .btn-text {
            line-height: 60px;
          }
          i {
            width: 12px;
            height: 12px;
            display: inline-block;
            background: url(../../image/search/type-plus-12x12.png?__sprite) no-repeat;
            margin-left: 4px;
            margin-top: 24;
          }
        }
      }

    }





}

/*公共样式*/
.dib { display: inline-block }
.color-666 { color: #666 }
.color-ddd { color: #ddd }
.color-fff { color: #fff }
.color-fa { color: #FAFAFA }
.color-d3 { color: #D3D3D3 };
.clearfix:after {
  visibility: hidden;
  display: block;
  font-size: 0;
  content: " ";
  clear: both;
  height: 0
}
