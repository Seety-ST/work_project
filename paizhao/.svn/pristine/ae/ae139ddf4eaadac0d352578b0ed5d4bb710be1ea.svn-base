.index-page{
	.item-wrap{
		.search-item{
			padding:0.35rem 0.75rem;
			display: -webkit-box;
			.lbox{
				position:relative;
				width:4.05rem;
				.item{
					padding:0.25rem 1.25rem 0px 0px;
					position:relative;
					.arrow-icon{
						width:0.5rem;
						height: 0.5rem;
						position: absolute;
						right:0.5rem;
						top:0.5rem;
						display:inline-block;
						vertical-align: middle;
						background-image: url(../../image/index/arrow-icon-hover-20x20.png?__inline);
						background-size: 0.5rem 0.5rem;
						&.arrow-icon-hover{
							transform: rotateX(180deg);
							-webkit-transform: rotateX(180deg);

						}
					}
					.txt{
						

					}
				}
			
				.pop-item{
					background-color: #fff;
					position:absolute;
					left: 0;
					
					z-index: 99;
					box-shadow: 0 0 1rem rgba(0, 0, 0, 0.5);
					&.top42{
						top:2.1rem;
					}
					&.top011{
						top:-6rem;
					}
					.triangle-icon{
							width: 0;
							height: 0;
							border-left: 10px solid transparent;
							border-right: 10px solid transparent;
							border-bottom: 10px solid #fff;
							position: absolute;
							top: -0.5rem;
							left: 50%;
							margin-left: -0.5rem;
					}
					.list{
						padding:0px 0.5rem;
						li{
							padding:0.65rem 0px;
							border-bottom: 1px solid #ddd;
							&:last-child{
								border-bottom:none;
							}
						}
					}
				}
				
			}
			.rbox{
				-webkit-box-flex:1;
				.input-txt{
					padding:0px 4%;
					width:92%;
					height:1.5rem;
					background-color: #eeeeee;
				}
			}
		}
		.content{
			.p1th{
				.swiper-container{
					position:relative;
					.swiper-wrapper{
						.swiper-slide{
							.img{
								width:100%;
								min-height: 8rem;
								display: block;
							}
						}
					}
					
					.swiper-pagination{
						width:100%;
						position:absolute;
						left:0;
						bottom:2.667%;
						text-align: center;
						.swiper-pagination-bullet{
							width:0.4rem;
							height:0.4rem;
							opacity:1;
							border-radius: 50%;
							display:inline-block;
							margin-right: 0.15rem;
							background: rgba(0,0,0,.1);
							border:1px solid transparent;
							&.swiper-pagination-bullet-active{
								border-color:#fff;
							}
						}
					}
				}
				
			}
			.p2th{
				background-color: #fff;
				.list{
					li{
						width:23.5%;
						
						float: left;
						position:relative;
						margin:1.45% 0px 0px 1.15%;
						.img{
							width:100%;
							padding-top: 100%;
							.img{
								width:100%;
								position:absolute;
								top:0;
								left:0;
								z-index: 1;
								display:block;
								background-size: 100%;
							}
						}
						.txt-item{
							width:100%;
							height:100%;
							position:absolute;
							left:0;
							top:0;
							z-index: 2;
							text-align: center;
							color:#fff;
							background: rgba(0,0,0,.4);
							
							.txt{
								width:100%;
								height:100%;
								display:table;
								span{
									display:table-cell;
									vertical-align: middle;
								}
							}
						}
						&.all-list-style{
							width:22.6%;
							border:2px solid #24c7c7;
							.img{
								img{
									min-height: 4.2rem;
								}
							}
							.txt-item{
								color:#24c7c7;
								
								background: rgba(255,255,255,.9);
								.txt{
									.arrow-icon{
										width:0.6rem;
										height:0.6rem;
										display:inline-block;
										background:url(../../image/index/arrow-icon-24x24.png?__inline) no-repeat   center;
										background-size: 0.6rem 0.6rem;
										background-position: 0px 0.05rem;
									}
								}
								
							}
						}
					}
				}
			}
			.p3th{
				padding:0.5rem 0px;
				background-color: #fff;
				.list{
					li{
						width:33.33%;
						float:left;
						&.even{
							width:50%
						}
						&.odd{
							width:33.33%;
						}
						.img-item{
							width:35.2%;
							padding-top: 35.2%;
							position:relative;
							margin:0.5rem auto;
							.img{
								width:100%;
								height:100%;
								position:absolute;
								left:0;
								top:0;
								background-size: cover;
							}
						}
						.txt-item{
							text-align: center;
							margin-bottom: 0.5rem;
							overflow: hidden;
							text-overflow: ellipsis;
							display: -webkit-box;
							-webkit-line-clamp: 1;
							-webkit-box-orient: vertical;
							color:#44454a;
						}
					}
				}
			}
			
			.p4th{
				padding:0.75rem 0px;
				margin-top: 0.5rem;
				background-color: #fff;
				.title-item{
					padding:0px 0.75rem;
					.lbox{
						.en-txt{
							padding-bottom: 0.35rem;
							background: url(../../image/index/line-img-40x4.png?__inline) no-repeat left bottom;
							background-size: 1rem 0.1rem;

						}
						.cn-txt{
							padding-top: 0.35rem;
							font-weight: bold;
						}
						
					}
					.rbox{
						padding-top: 1.5rem;
						.jiantou-icon{
							width:0.8rem;
							height:0.8rem;
							display:inline-block;
							vertical-align: middle;
							background: url(../../image/index/jiantou-icon-32x32.png?__inline) no-repeat center;
							background-size: 100%;
						}
						.more{
							display:inline-block;
							vertical-align: middle;
						}
					}
				}
				.item-wrap{
					
					.swiper-container{
						.swiper-wrapper{
							.swiper-slide{
								width:50%;
								margin-bottom: 0.8rem;
								margin-right:9%;
								box-shadow: 0 0 0.75rem rgba(0,0,0,.2);
								opacity: .3;
								margin-top: 1rem;
								.img-item{
									width:100%;
									padding-top: 100%;
									position:relative;
									.img{
										width:100%;
										height:100%;
										position:absolute;
										top:0;
										left:0;
										background-size: cover;
										background-position: center;
									}
								}
								.txt-item{
									padding:0.5rem;
									display:-webkit-box;
									.lbox{
										width:2.25rem;
										height:2.25rem;
										.avatar-icon{
											width:100%;
											img{
												width:100%;
												display:block;
											}
										}
									}
									.rbox{
										-webkit-box-flex:1;
										padding:0px 3%;
										text-align: center;
										.box-1{
											overflow: hidden;
											text-overflow: ellipsis;
											display: -webkit-box;
											-webkit-line-clamp: 1;
											-webkit-box-orient: vertical;
										}
										.box-2{
											padding-top: 0.25rem;
											.tips-txt{
												padding:0.1rem;
												font-size: 0.45rem;
												background-color: #24c9c9;
												color:#fff;
											}
										}
									}
								}
								&.swiper-slide-active{
									opacity:1;
								}
							}

						}
					}

				}
			}
			.p5th{
				padding:0.75rem 0px 0px 0px;
				margin-top: 0.5rem;
				background-color: #fff;
				.title-item{
					padding:0px 0.75rem;
					.lbox{
						.en-txt{
							padding-bottom: 0.35rem;
							background: url(../../image/index/line-img-40x4.png?__inline) no-repeat left bottom;
							background-size: 1rem 0.1rem;

						}
						.cn-txt{
							padding-top: 0.35rem;
							font-weight: bold;
						}
						
					}
					.rbox{
						padding-top: 1.5rem;
						.jiantou-icon{
							width:0.8rem;
							height:0.8rem;
							display:inline-block;
							vertical-align: middle;
							background: url(../../image/index/jiantou-icon-32x32.png?__inline) no-repeat center;
							background-size: 100%;
						}
						.more{
							display:inline-block;
							vertical-align: middle;
						}
					}
				}
				.item-wrap{
					padding-top: 1.25rem;
					padding:1.25rem 0px 1rem 0.75rem;
					overflow-x:auto;
					.list{
						display:-webkit-box;
						a{
							width:100%;
						}
						li{
							width:38.9%;
							margin-right: 2.6%;
							.con-item{
								width:100%;
								.img-item{
									width:100%;
									padding-top: 100%;
									position:relative;
									.img{
										width:100%;
										height:100%;
										display:inline-block;
										position:absolute;
										left:0;
										top:0;
										background-size: cover;
									}
								}
								.txt-item{
									position:relative;
									.box-1{
										padding-top: 1.2rem;
										.ui-avatar-icon-45{
											width:2.25rem;
											height:2.25rem;
										}
										.ui-avatar-icon{
											position:absolute;
											top: -1.15rem;
											left: 50%;
											margin-left: -1.15rem;
											i{
												width:100%;
												height:100%;
												display: block;
												background-size: cover;
												border-radius: 50%;
												border:2px solid #fff;
											}
										}
									}
									.box-2{
										.txt-1{
											padding:0.3rem 0px 0.3rem 0px;
											background: url(../../image/index/line-img-40x2.jpg?__inline) no-repeat center bottom;
											background-size: 1rem 0.05rem 
										}
									}
									.box-3{
										.txt-1{
											padding-top: 0.2rem;
										}
									}
								}
							}
						}
					}
				}
			}
			.p6th{
				padding-top:0.75rem;
				margin: 0.5rem 0px 1.5rem 0px;
				background-color: #fff;
				.title-item{
					padding:0px 0.75rem;
					.lbox{
						.en-txt{
							padding-bottom: 0.35rem;
							background: url(../../image/index/line-img-40x4.png?__inline) no-repeat left bottom;
							background-size: 1rem 0.1rem;

						}
						.cn-txt{
							padding-top: 0.35rem;
							font-weight: bold;
						}
						
					}
					
				}
				.more-item{
					padding:0.75rem 0px;
					text-align: center;
					background-color: #fff;
					border-top:1px solid #ddd;
					.jiantou-icon{
						width:0.8rem;
						height:0.8rem;
						display:inline-block;
						vertical-align: middle;
						background: url(../../image/index/jiantou-icon-32x32.png?__inline) no-repeat center;
						background-size: 100%;
					}
					.more{
						display:inline-block;
						vertical-align: middle;
					}
				}
				.item-wrap{
					padding:1rem 0px 0px 0.75rem;
					.list{
						overflow: hidden;
						li{
							padding-bottom: 0.75rem;
							margin-right: 4%;
							float: left;
							.img-item{
								width:100%;
								padding-top: 100%;
								position:relative;
								.img{
									width:100%;
									height:100%;
									display:inline-block;
									position:absolute;
									left:0;
									top:0;
									background-size: cover;
								}
								.txt{
									position:absolute;
									right:0;
									bottom:0;  
									background:rgba(0,0,0,.5);
									padding:1% 1.4%;
								}
							}
							.txt-item{
								.box-1{
									.txt-1{
										padding-top: 0.5rem;
										height:1.95rem;
										overflow: hidden;
										text-overflow: ellipsis;
										display: -webkit-box;
										-webkit-line-clamp: 2;
										-webkit-box-orient: vertical;
									}
								}
								.box-2{
									.txt-1{
										padding-top: 0.25rem;
									}
								}
							}
							&.max-size{
								width:96%;
								.txt-item{
									.box-1{
										text-align: center;
										.txt-1{
											height:1.05rem;
											-webkit-line-clamp: 1;
										}
									}
									.box-2{
										text-align: center;
									}
								}
							}
							&.min-size{
								width:46%;
							}
						}
					}

				}
			}
		}
	}
}