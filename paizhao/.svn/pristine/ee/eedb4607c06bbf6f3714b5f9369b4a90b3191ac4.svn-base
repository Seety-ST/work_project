    function change_status(id,status)
    {
        if(!confirm('是否确认修改状态？'))
        {
            return;
        }
        $.ajax( {
            url:'?action=goods_chstatus&id='+id+'&status='+status,
            type:'post',
            cache:false,
            dataType:'json',
            success:function(data) {
                alert(data.message);
                /*
                 if(data.result==1){
                 alert('修改成功');
                 location.reload();
                 }else{
                 alert(data.message);
                 }
                 */
            },
            error : function() {
                alert("异常！");
            }
        });
    }
</script>
