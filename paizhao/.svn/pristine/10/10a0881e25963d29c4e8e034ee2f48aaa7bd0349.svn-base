.go-top-mod a {width: 44px;
  height: 44px;
  display: block;
  position: fixed;
  right: 60px;
  bottom: 60px;
  text-decoration: none;
  color: #fff;
  
  width: 45px;
  height: 45px;
  -webkit-transition: all 0.2s;
  -moz-transition: all 0.2s;
  -o-transition: all 0.2s;
  -ms-transition: all 0.2s;
  transition: all 0.2s;background-repeat: no-repeat;}
  .go-top-mod a:hover {background-repeat: no-repeat;}
.go-top-mod a{background-position:0px 0px}.go-top-mod a:hover{background-position:0px -54px}.go-top-mod a,.go-top-mod a:hover{background-size: 44px 98px;background-image: url(http://paizhao.yueus.com/static/pc/modules/go_top/go_top_1_z.png)}