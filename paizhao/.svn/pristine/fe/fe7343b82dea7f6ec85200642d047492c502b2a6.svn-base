@charset "UTF-8";
.w1200 {
  width: 1200px;
  margin: 0 auto; }

.content-wrap .select-nav {
  height: 50px; }
  .content-wrap .select-nav .select-nav-text {
    line-height: 50px; }
  .content-wrap .select-nav .r-arrow {
    margin-left: 15px;
    margin-right: 15px; }
  .content-wrap .select-nav .main-type {
    position: relative;
    z-index: 5; }
    .content-wrap .select-nav .main-type .main-type-list {
      display: none;
      position: absolute;
      height: 40px;
      line-height: 40px;
      top: 25px;
      border: 1px solid #49DED7;
      z-index: 4; }
      .content-wrap .select-nav .main-type .main-type-list .main-select-disable {
        color: #ddd; }
      .content-wrap .select-nav .main-type .main-type-list .main-select-able {
        color: #666; }
      .content-wrap .select-nav .main-type .main-type-list .main-select-able:hover {
        color: #49DED7; }
  .content-wrap .select-nav .main-type:hover .main-type-list {
    display: block; }
  .content-wrap .select-nav .select-nav-tab {
    height: 22px;
    border: 1px solid #ddd;
    margin-left: 10px;
    margin-top: 14px; }
    .content-wrap .select-nav .select-nav-tab .tab-name {
      display: inline-block;
      padding: 5px 10px; }
    .content-wrap .select-nav .select-nav-tab .tab-close {width: 12px;
      height: 12px;
      display: inline-block;
      
      margin: 5px 10px 5px 10px;background-repeat: no-repeat;}
    .content-wrap .select-nav .select-nav-tab .tab-arrow {width: 12px;
      height: 12px;
      display: inline-block;
      
      margin: 5px 10px 5px 10px;background-repeat: no-repeat;}
  .content-wrap .select-nav .select-nav-tab-hover {
    border: 1px solid #49DED7;
    border-bottom: none; }
    .content-wrap .select-nav .select-nav-tab-hover .tab-arrow {background-repeat: no-repeat;}
.content-wrap .select-list {
  border: 1px solid #ddd; }
  .content-wrap .select-list .border-line {
    border-bottom: 1px solid #ddd; }
  .content-wrap .select-list .select-list-tab {
    height: 50px; }
    .content-wrap .select-list .select-list-tab .tab-title {
      width: 109px;
      background-color: #FAFAFA;
      line-height: 50px;
      padding-right: 21px; }
    .content-wrap .select-list .select-list-tab .tab-list {
      margin-left: 20px; }
      .content-wrap .select-list .select-list-tab .tab-list .list li {
        float: left;
        line-height: 50px;
        margin-left: 20px; }
    .content-wrap .select-list .select-list-tab .tab-btn {
      width: 60px; }
      .content-wrap .select-list .select-list-tab .tab-btn .btn-text {
        line-height: 60px; }
      .content-wrap .select-list .select-list-tab .tab-btn i {width: 12px;
        height: 12px;
        display: inline-block;
        
        margin-left: 4px;
        margin-top: 24;background-repeat: no-repeat;}

/*公共样式*/
.dib {
  display: inline-block; }

.color-666 {
  color: #666; }

.color-ddd {
  color: #ddd; }

.color-fff {
  color: #fff; }

.color-fa {
  color: #FAFAFA; }

.color-d3 {
  color: #D3D3D3; }

.clearfix:after {
  visibility: hidden;
  display: block;
  font-size: 0;
  content: " ";
  clear: both;
  height: 0; }
.content-wrap .select-nav .select-nav-tab .tab-close{background-position:0px 0px}.content-wrap .select-nav .select-nav-tab .tab-arrow{background-position:0px -22px}.content-wrap .select-nav .select-nav-tab-hover .tab-arrow{background-position:0px -44px}.content-wrap .select-list .select-list-tab .tab-btn i{background-position:0px -66px}.content-wrap .select-nav .select-nav-tab .tab-close,.content-wrap .select-nav .select-nav-tab .tab-arrow,.content-wrap .select-nav .select-nav-tab-hover .tab-arrow,.content-wrap .select-list .select-list-tab .tab-btn i{background-size: 12px 78px;background-image: url(http://paizhao.yueus.com/static/pc/style/search/search-page_1_z.png)}