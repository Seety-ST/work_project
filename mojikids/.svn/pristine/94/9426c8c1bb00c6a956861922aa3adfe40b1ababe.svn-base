<script type="text/javascript">
    $(function() {


        // 加载util
        var Util = require.syncLoad('wap-kids:global/util/index');

        $('#btn_use').on('click', function(event) {

            // 异步请求
            Util.request
            ({
                url : window.__MOJIKIDS_GLOBAL['AJAX_DOMAIN'] + 'topic/t_27/get_code.php',
                type : 'POST',
                cache : true,
                data :
                {
                    c : '{%$c%}',
                    v : '{%$v%}',
                    g : '{%$g%}',
                    p : '{%$p%}'
                },
                beforeSend : function()
                {
                    Util.loading.open
                    ({
                        text : '加载中...',
                        timeout:2000
                    });
                },
                success : function(result)
                {
                    Util.loading.close();
                    console.log(result);
                    var ret = result.res


                    if (ret.result == 1) 
                    {
                        console.log('跳转');  
                        window.location.href = window.__MOJIKIDS_GLOBAL['PAGE_URL']['order'] + "date.php?goods_id={%$g%}&standard_id={%$p%}&c={%$c%}&v={%$v%}&buy_num=1&store_id={%$store_id%}&schedule_id={%$schedule_id%}"
                    }

                    else
                    {
                        // Util.toast
                        // ({
                        //     message: ret.message,
                        //     position: 'middle',
                        //     duration: 3000
                        // });

                        Util.dialog({
                            title: 'oh，你已经来晚了，邀请码已经失效了。',
                            message: '',
                            showCancelButton : false,
                            confirmButtonText : '好吧，我知道了',
                            cancelButtonText : ''
                        },
                        /**
                         * 按钮点中后的回调
                         * @param  {[string]} value [description]
                         * @return {[string]} 返回confirm  || cancel [description]
                         */
                        function(value)
                        {
                            console.log(value);
                        })
                        return;
                    }
                  
                }
            });
        });

    

        // 分享
        window.__MOJIKIDS_GLOBAL['share_com']
        // 公共底部已经实例化，不需要重新实例
        // 默认分享内容已经加上
        // 支持App 微信 QQ 分享

        // 对应页面修改分享内容，如下
        var __SHARE_TEXTS = JSON.parse('{%$share_text%}');
        window.__MOJIKIDS_GLOBAL['share_com'].change_share_text(__SHARE_TEXTS.res);
        // __SHARE_TEXTS 结构格式
    
        $('#share').on('click', function(event) {
            console.log('分享');
            window.__MOJIKIDS_GLOBAL['share_com'].show_layer();
        });

        $('[data-role="share-fade"]').on('click', function(event) {
            window.__MOJIKIDS_GLOBAL['share_com'].hide_layer();
        });

    });
</script>

