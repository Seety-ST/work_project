.mine-page .p1th .item-wrap {
  padding: 0.15rem;
}

.mine-page .p1th .item-wrap .txt-item {
  margin-left: 0.1rem;
}

.mine-page .p1th .item-wrap .ui-avatar-icon i {
  background-position: center;
  background-repeat: no-repeat;
  background-size: contain;
  background-image: url(http://mojikids.yueus.com/static/wap/image/mine/user-img-defalut.png);
}

.mine-page .p2th .item-wrap .title-item {
  margin-top: 0.12rem;
}

.mine-page .p2th .item-wrap .title-item .arrow-icon {
  width: 0.16rem;
  height: 0.16rem;
  display: inline-block;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: contain;
  margin-left: 0.04rem;
  vertical-align: middle;
}

.mine-page .p2th .item-wrap .title-item .text {
  display: inline-block;
  vertical-align: middle;
}

.mine-page .p2th .item-wrap .content .no-data-item {
  width: 2.95rem;
  height: 1.4rem;
  margin: 0.15rem auto;
  border-radius: 0.08rem;
  box-shadow: 0 0 0.15rem rgba(0, 0, 0, 0.15);
  background: url(http://mojikids.yueus.com/static/wap/image/mine/no-data-bg-120x120.5e01878.jpg) no-repeat 2.2rem bottom;
  background-size: 0.6rem 0.6rem;
}

.mine-page .p2th .item-wrap .content .no-data-item .add-btn-icon {
  width: 0.6rem;
  height: 0.6rem;
  display: block;
  margin: 0 auto;
  padding-top: 0.25rem;
  background: url(http://mojikids.yueus.com/static/wap/image/mine/add-btn-114x114.png) no-repeat center bottom;
  background-size: contain;
}

.mine-page .p2th .item-wrap .content .no-data-item .text {
  padding-top: 0.15rem;
}

.mine-page .p2th .item-wrap .content .baby-list-item {
  overflow-x: auto;
}

.mine-page .p2th .item-wrap .content .baby-list-item .list {
  padding-left: 0.4rem;
  display: -webkit-box;
}

.mine-page .p2th .item-wrap .content .baby-list-item .list li {
  width: 2.95rem;
  height: 1.4rem;
  overflow: hidden;
  position: relative;
  border-radius: 0.08rem;
  box-shadow: 0 0 0.15rem rgba(0, 0, 0, 0.15);
  margin: 0.15rem 0.15rem 0.15rem 0;
}

.mine-page .p2th .item-wrap .content .baby-list-item .list li .ui-avatar-icon {
  margin: 0.15rem auto 0;
}

.mine-page .p2th .item-wrap .content .baby-list-item .list li .ui-avatar-icon i {
  background-size: 100%;
  background-repeat: no-repeat;
  background-position: center;
}

.mine-page .p2th .item-wrap .content .baby-list-item .list li .txt-item .box-1 {
  padding-top: 0.1rem;
}

.mine-page .p2th .item-wrap .content .baby-list-item .list li .txt-item .box-2 {
  padding-top: 0.04rem;
}

.mine-page .p2th .item-wrap .content .baby-list-item .list li .girl-bg-1 {
  background: url(data:image/png;base64,) no-repeat center;
}

.mine-page .p2th .item-wrap .content .baby-list-item .list li .girl-bg-2 {
  background: url(data:image/png;base64,) no-repeat center;
}

.mine-page .p2th .item-wrap .content .baby-list-item .list li .boy-bg-1 {
  background: url(data:image/png;base64,) no-repeat center;
}

.mine-page .p2th .item-wrap .content .baby-list-item .list li .boy-bg-2 {
  background: url(data:image/png;base64,) no-repeat center;
}

.mine-page .p2th .item-wrap .content .baby-list-item .list li .li-bg-1 {
  background-size: cover;
  position: absolute;
  left: 0.1rem;
  top: 0.1rem;
}

.mine-page .p2th .item-wrap .content .baby-list-item .list li .li-bg-2 {
  background-size: cover;
  position: absolute;
  right: 0.1rem;
  bottom: 0.1rem;
}

.mine-page .p2th .item-wrap .content .baby-list-item .list li:nth-last-child(1) {
  margin-right: 0;
}

.mine-page .p3th .item-wrap .ui-edit-user-info-mod .user-info-item .lbox .tips {
  margin-left: 0;
}