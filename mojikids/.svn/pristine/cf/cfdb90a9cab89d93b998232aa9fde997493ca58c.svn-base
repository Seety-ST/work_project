  background: url(data:image/png;base64,) no-repeat center;
  background-size: cover;
}

.index-page .p3th .item-wrap .no-data-item {
  width: 3.45rem;
  height: 1.4rem;
  margin: 0.15rem auto 0.2rem;
  border-radius: 0.08rem;
  box-shadow: 0 0 0.15rem rgba(0, 0, 0, 0.15);
  background: url(http://mojikids.yueus.com/static/wap/image/mine/no-data-bg-120x120.5e01878.jpg) no-repeat right bottom;
  background-size: 0.6rem 0.6rem;
}

