  position: relative;
}

.index-page .p3th .item-wrap .baby-list-item .list li .img-item {
  position: relative;
  width: 0.4rem;
  height: 0.4rem;
  margin: 0 auto;
}

.index-page .p3th .item-wrap .baby-list-item .list li .img-item .ui-avatar-icon i {
  background-size: 100%;
  background-position: center;
  background-repeat: no-repeat;
}

.index-page .p3th .item-wrap .baby-list-item .list li .img-item .mark-icon {
  width: 0.32rem;
  height: 0.32rem;
  position: absolute;
  top: -0.1rem;
  left: -0.2rem;
  z-index: 1;
  background-repeat: no-repeat;
  background-size: contain;
  background-position: center;
}

.index-page .p3th .item-wrap .baby-list-item .list li .img-item .mark-icon.girl-mark-icon {
  background-image: url(data:image/png;base64,);
}

.index-page .p3th .item-wrap .baby-list-item .list li .img-item .mark-icon.boy-mark-icon {
  background-image: url(data:image/png;base64,);
}

.index-page .p3th .item-wrap .baby-list-item .list li .txt-item {
