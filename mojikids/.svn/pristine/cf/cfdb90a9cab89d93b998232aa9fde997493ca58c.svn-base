.index-page .p1th {
  background-color: #fff;
}

.index-page .p1th .item-wrap .img-item {
  height: 1.6rem;
  overflow: hidden;
}

.index-page .p1th .item-wrap .img-item img {
  width: 100%;
  height: 1.6rem;
  display: block;
  object-fit: cover;
}

.index-page .p1th .item-wrap .img-item .swiper-pagination {
  width: 98%;
  text-align: right;
}

.index-page .p1th .item-wrap .img-item .swiper-pagination .swiper-pagination-bullet {
  width: 0.09rem;
  height: 0.09rem;
  background-color: #f4f4f3;
  opacity: 1;
  border: 1px solid #d8d7d7;
}

.index-page .p1th .item-wrap .img-item .swiper-pagination .swiper-pagination-bullet-active {
  background-color: #ffc430;
}

.index-page .p2th {
  position: relative;
}

.index-page .p2th .item-wrap {
  padding-top: 0.07rem;
}

.index-page .p2th .item-wrap .address-item {
  width: 100%;
  position: absolute;
  left: 0;
  top: -0.19rem;
  z-index: 1;
  padding-top: 0.13rem;
  background: url(data:image/png;base64,) no-repeat center top;
  background-size: 0.98rem 0.2rem;
  text-align: center;
}

.index-page .p2th .item-wrap .address-item .address-icon {
  width: 0.16rem;
  height: 0.16rem;
  display: inline-block;
  vertical-align: middle;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: cover;
}

.index-page .p2th .item-wrap .address-item .address-txt {
  vertical-align: middle;
  color: #ff5669;
}

.index-page .p3th .item-wrap .baby-list-item .list li {
