  background: url(http://mojikids.yueus.com/static/wap/image/mine/add-btn-114x114.png) no-repeat center bottom;
  background-size: contain;
}

.index-page .p3th .item-wrap .no-data-item .text {
  padding-top: 0.15rem;
}

.index-page .p4th .item-wrap {
  padding: 0 0.15rem;
}

.index-page .p4th .item-wrap .list li {
  padding: 0.1rem 0.05rem 0.1rem 0.1rem;
  box-shadow: 0 0 0.15rem rgba(0, 0, 0, 0.15);
  border-radius: 0.08rem;
  margin-bottom: 0.2rem;
}

.index-page .p4th .item-wrap .list li .contain {
  background: url(data:image/jpeg;base64,) no-repeat center bottom;
  background-size: contain;
}

.index-page .p4th .item-wrap .list li .contain .img-item .img {
  width: 1.05rem;
  height: 1.05rem;
  display: block;
  float: left;
  margin: 0 0.05rem 0.05rem 0;
  object-fit: cover;
  background-position: center;
  background-size: 100%;
  background-repeat: no-repeat;
}

.index-page .p4th .item-wrap .list li .contain .img-item .img:first-child {
  width: 3.25rem;
  height: 1.35rem;
}

.index-page .p4th .item-wrap .list li .contain .txt-item .box-1 {
  padding-top: 0.06rem;
}

.index-page .p4th .item-wrap .list li .contain .txt-item .box-2 {
