.photos-page{
    .top-bar{
        background-color: #fff3d6;
        height: 40px;
        line-height: 40px;
        color:#ffc430;
        position: relative;
        .delete{
            background-image: url('data:image/png;base64,');
            background-size: contain;
            width: 12px;
            height: 12px;
            display: inline-block;
            vertical-align: middle;
            position: absolute;
            top: 13px;
            right: 15px;
            z-index: 1;
        }
    }
    .list-wrapper{
        padding-bottom: 45px;
        .container{
            display: inline-block;;
            .item{
                display: -webkit-box;
                font-size: 0;
                float: left;
                box-sizing: border-box;
                width: calc((100% - (1px+1px)* 3 * 2) / 3 );
                height: calc((100% - (1px+1px)* 3 * 2) / 3 );
                margin-bottom: 2px;
                background-color: #e6e6e6;
                &:nth-child(3n-1){
                    margin: 0 2px;
                }
                .child{
                    position: relative;
                    display: -webkit-box;
                    -webkit-box-pack: center;
                    -webkit-box-align: center;
                    img{
                        width: 100%;
                    }
                    .extend{
                        position: absolute;
                        width: 100%;
                        height: 100%;
                        top: 0;
                        left:0;
                        .fade-mix{
                            background-color: rgba(0,0,0,.4);
                            width: 100%;
                            height: 100%;
                        }
                        .circle{

                            background-color: #ffc430;
                            color:#333;
                            font-size: 14px;
                            width: 22px;
                            height: 22px;
                            position: absolute;
                            right: 10px;
                            top: 10px;
                            border-radius: 50%;
                            text-align: center;
                            line-height: 22px;
                            em{
                            }
                        }
                        .none{

                            position: absolute;
                            right: 10px;
                            top: 10px;
                            width: 22px;
                            height: 22px;
                            .big{
                                width: 22px;
                                height: 22px;
                                border-radius: 50%;
                                border: 1px solid #fff;
                                background-color: rgba(0,0,0,.4);
                            }
                        }
                    }
                }
            }
        }
    }

    .footer-bar{
        .icon-zzhb{
            background-image: url('data:image/png;base64,');
            width: 19px;
            height: 19px;
            display: inline-block;
            font-size: 0;
            vertical-align: middle;
            background-size: contain;
            margin-right: 6px;
            position: relative;
            top: -2px;
        }
        .icon-wysq{
            background-image: url('data:image/png;base64,');
            width: 19px;
            height: 19px;
            display: inline-block;
            font-size: 0;
            vertical-align: middle;
            background-size: contain;
            margin-right: 6px;
            position: relative;
            top: -2px;
        }
        .line{
            width: 1px;/*no*/
            background-color: #fff;
            background:linear-gradient(to top,#fff,#fff);
            margin: 15px 0;
        }
    }
}
