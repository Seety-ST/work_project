@charset "UTF-8";

.native-scroll {
  overflow: auto !important;
  -webkit-overflow-scrolling: touch !important;
}

/*列表头部角标*/

.ui-list-item-tips {
  display: block;
  position: absolute;
  left: -0.08rem;
  top: 0;
  width: 0.32rem;
  height: 0.16rem;
  background-color: #ff6a6e;
  color: #fff;
  text-align: center;
  line-height: 0.16rem;
  font-size: 0.1rem;
  border-radius: 0.02rem;
  z-index: 7;
}

.ui-list-item-tips .ui-list-item-tips-triangle {
  z-index: 2;
  width: 0;
  height: 0;
  border-top: 0.08rem solid #e25357;
  border-left: 0.08rem solid transparent;
  position: absolute;
  top: 0.15rem;
  left: 0;
}

/*列表icon遮罩*/

.ui-list-item-fade {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 3;
  background-color: rgba(0, 0, 0, 0.5);
  -webkit-box-pack: center;
  box-pack: center;
  display: -webkit-box;
  display: box;
  -webkit-box-align: center;
  box-align: center;
}

.global-footer-bar-item {
  width: 100%;
  background-color: #fffdfb;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 1;
}

.global-footer-bar-item .item-wrap {
  padding: 0.06rem 0.15rem 0.04rem 0.15rem;
}

.global-footer-bar-item .item-wrap .list {
  display: -webkit-box;
  display: -moz-box;
  display: -o-box;
  display: -ms-flexbox;
  display: box;
}

.global-footer-bar-item .item-wrap .list a {
  -webkit-box-flex: 1;
  -moz-box-flex: 1;
  -ms-flex: 1;
  -o-box-flex: 1;
  box-flex: 1;
  display: block;
}

.global-footer-bar-item .item-wrap .list li .footer-icon {
  width: 0.27rem;
  height: 0.27rem;
  display: block;
  margin: 0 auto;
  background-position: center;
  background-size: cover;
  background-repeat: no-repeat;
}

.global-footer-bar-item .item-wrap .list li .footer-index-icon {
  background-image: url(data:image/png;base64,);
}

.global-footer-bar-item .item-wrap .list li .footer-order-icon {
  background-image: url(data:image/png;base64,);
}

.global-footer-bar-item .item-wrap .list li .footer-mine-icon {
  background-image: url(data:image/png;base64,);
}

.global-footer-bar-item .item-wrap .list li.cur .footer-txt {
  color: #000;
}

.global-footer-bar-item .item-wrap .list li.cur .footer-index-icon {
  background-image: url(data:image/png;base64,);
}

.global-footer-bar-item .item-wrap .list li.cur .footer-order-icon {
  background-image: url(data:image/png;base64,);
}

.global-footer-bar-item .item-wrap .list li.cur .footer-mine-icon {
  background-image: url(data:image/png;base64,);
}