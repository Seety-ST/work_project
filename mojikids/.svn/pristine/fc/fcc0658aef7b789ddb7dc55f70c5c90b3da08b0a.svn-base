@import "../../sass/compass";
//`iconfont 汤圆 2016-1-18

// 扩展方法
@mixin yue-ui-icon-font-size($size)
{
    font-size: $size !important;
}

@mixin yue-ui-icon-size($size)
{
    width: $size;
    height: $size;
    display:  block;
}


