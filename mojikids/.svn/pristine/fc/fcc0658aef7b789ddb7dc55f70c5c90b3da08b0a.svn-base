.icon-font-size-22{@include yue-ui-icon-font-size(22px)}
.icon-font-size-30{@include yue-ui-icon-font-size(30px)}
.icon-font-size-32{@include yue-ui-icon-font-size(32px)}
.icon-font-size-43{@include yue-ui-icon-font-size(43px)}
