.mint-popup {
  background: transparent !important;
}

.receive-explain-pop .close-btn {
  width: 0.33rem;
  height: 0.33rem;
  position: absolute;
  left: 50%;
  bottom: -0.55rem;
  margin-left: -0.16rem;
  display: inline-block;
  background: url(http://mojikids.yueus.com/static/wap/image/topic/open_store/close-img.png) no-repeat center;
  background-size: 100%;
}

.receive-explain-pop .item-wrap {
  width: 3.18rem;
  background-size: 100% auto;
}

.receive-explain-pop .item-wrap .title-item {
  font-weight: bold;
  width: 100%;
  height: 0.78rem;
  background: url(http://mojikids.yueus.com/static/wap/image/topic/open_store/pop-title-636x155.png) no-repeat center;
  background-size: 100%;
}

.receive-explain-pop .item-wrap .title-item .title {
  padding-top: 0.1rem;
}

.receive-explain-pop .item-wrap .item-1 {
  padding: 0.27rem 0.18rem 0.2rem 0.3rem;
  background: url(http://mojikids.yueus.com/static/wap/image/topic/open_store/pop-bg-636x656.png) no-repeat center bottom;
  background-size: 100%;
  background-color: #fff;
  border-bottom-left-radius: 0.2rem;
  border-bottom-right-radius: 0.2rem;
  position: relative;
}

.receive-explain-pop .item-wrap .item-1 li {
  padding-bottom: 0.3rem;
  padding-left: 0.12rem;
}

.receive-explain-pop .item-wrap .item-1 li .txt-item {
  display: -webkit-box;
  padding-left: 0.08rem;
  font-size: 0.11rem;
  position: relative;
}

.receive-explain-pop .item-wrap .item-1 li .txt-item .cicle-item {
  width: 0.27rem;
  height: 0.27rem;
  text-align: center;
  line-height: 0.29rem;
  background: url(http://mojikids.yueus.com/static/wap/image/topic/open_store/circle-img-55x54.png) no-repeat center;
  background-size: 100%;
  position: absolute;
  left: -0.26rem;
  top: 50%;
  margin-top: -0.13rem;
  z-index: 2;
}

.receive-explain-pop .item-wrap .item-1 li .txt-item .txt-1 {
  -webkit-box-flex: 1;
}

@-webkit-keyframes loadingimg {
  0% {
    top: -0.3rem;
    right: -0.2rem;
    -webkit-transform: rotate(0deg);
  }

  5% {
    top: -0.35rem;
    right: 0;
    -webkit-transform: rotate(-2deg);
  }

  10% {
    top: -0.36rem;
    right: 0.2rem;
    -webkit-transform: rotate(-4deg);
  }

  15% {
    top: -0.37rem;
    right: 0.4rem;
    -webkit-transform: rotate(-6deg);
  }

  20% {
    top: -0.36rem;
    right: 0.6rem;
    -webkit-transform: rotate(-8deg);
  }

  25% {
    top: -0.35rem;
    right: 0.8rem;
    -webkit-transform: rotate(-10deg);
  }

  30% {
    top: -0.3rem;
    right: 1rem;
    -webkit-transform: rotate(-12deg);
  }

  35% {
    top: -0.25rem;
    right: 1.2rem;
    -webkit-transform: rotate(-14deg);
  }

  40% {
    top: -0.19rem;
    right: 1.4rem;
    -webkit-transform: rotate(-16deg);
  }

  45% {
    top: -0.15rem;
    right: 1.6rem;
    -webkit-transform: rotate(-18deg);
  }

  50% {
    top: -0.13rem;
    right: 1.8rem;
    -webkit-transform: rotate(-20deg);
  }

  55% {
    top: -0.1rem;
    right: 2rem;
    -webkit-transform: rotate(-22deg);
  }

  60% {
    top: -0.12rem;
    right: 2.2rem;
  }

  65% {
    top: -0.14rem;
    right: 2.4rem;
  }

  70% {
    top: -0.16rem;
    right: 2.6rem;
  }

  75% {
    top: -0.18rem;
    right: 2.8rem;
  }

  80% {
    top: -0.2rem;
    right: 3rem;
  }

  85% {
    top: -0.22rem;
    right: 3.2rem;
  }

  90% {
    top: -0.24rem;
    right: 3.4rem;
  }

  95% {
    top: -0.26rem;
    right: 3.6rem;
  }

  100% {
    top: -0.28rem;
    right: 3.8rem;
  }
}