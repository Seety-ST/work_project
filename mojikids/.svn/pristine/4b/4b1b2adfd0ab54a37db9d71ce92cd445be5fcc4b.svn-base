.ui-dialog{position:fixed;top:0;left:0;width:100%;height:100%;z-index:999;display:-webkit-box;-webkit-box-orient:horizontal;-webkit-box-pack:center;-webkit-box-align:center;background:rgba(0,0,0,.4);display:none;color:#000}.ui-dialog-notice{background:transparent}.ui-dialog.show{display:-webkit-box;display:box}.ui-dialog .ui-dialog-cnt{border-radius:.06rem;width:2.7rem;-webkit-background-clip:padding-box;background-clip:padding-box;outline:0;pointer-events:auto;background-color:#fff;position:relative}.ui-dialog.ui-dialog-notice.show.ui-model-less .ui-dialog-cnt{position:absolute;top:50%;left:50%;margin:-.55rem 0 0 -.65rem}.ui-dialog .ui-dialog-bd{min-height:.71rem;border-top-left-radius:.06rem;border-top-right-radius:.06rem;padding:0 .18rem;font-size:.16rem;display:-webkit-box;display:box;-webkit-box-pack:center;-webkit-box-align:center;-webkit-box-orient:vertical}.ui-dialog .ui-dialog-bd h4{width:100%;text-align:center;color:#333;font-size:.18rem;margin-top:.25rem;margin-bottom:.1rem}.ui-dialog .ui-dialog-bd .txt{font-size:.14rem;color:#666;margin-top:.09rem;margin-bottom:.35rem}.ui-dialog .ui-dialog-bd div{width:100%;text-align:center}.ui-dialog .ui-dialog-ft{border-bottom-left-radius:.06rem;border-bottom-right-radius:.06rem}.ui-dialog .ui-dialog-ft button{height:.42rem;line-height:.42rem;background:transparent;font-size:.16rem;color:#333}.ui-dialog .ui-dialog-ft button:active{background:rgba(0,0,0,.1);color:#333}.ui-dialog .ui-dialog-ft button:first-child{border-bottom-left-radius:.06rem;background-image:none}.ui-dialog .ui-dialog-ft button:last-child{border-bottom-right-radius:.06rem}.ui-dialog-notice .ui-dialog-cnt{width:1.3rem;height:1.1rem;display:-webkit-box;-webkit-box-orient:vertical;-webkit-box-pack:center;-webkit-box-align:center;text-align:center;background:rgba(0,0,0,.65);border-radius:.06rem;color:#fff}